{
  "shaders": "compiledShaders/game",
  "assets": {
    "export": {
      "cache": "/../develop/.cache",
      "assetCache": "/../develop/.asset-local",
      "useCache2Fast": true,
      "types": {
        "type": [
          "tex",
          "rendInst",
          "collision",
          "physObj",
          "dynModel",
          "skeleton",
          "animChar",
          "animTree",
          "composit",
          "prefab",
          "fastphys",
          "gameObj",
          "a2d",
          "land",
          "tifMask",
          "spline",
          "mat",
          "proxymat",
          "apexDestr",
          "fx",
          "lshader",
          "impostorData"
        ]
      },
      "plugins": {
        "folder": "*common"
      },
      "folderDefaults": {
        "ddsxTexPack": "*parent",
        "gameResPack": "*parent"
      }
    },
    "build": {
      "rendInst": {
        "ignoreMappingInPrepareBillboardMesh": true,
        "descListOutPath": "riDesc",
        "separateModelMatToDescBin": true,
        "forceRiExtra": true,
        "impostorParamsOutPath": "riImpostors",
        "remapShaders": {
          "glass": "rendinst_simple_glass",
          "glass_refraction": "rendinst_refraction",
          "gi_black": "",
          "simple": [
            "rendinst_simple",
            "rendinst_simple"
          ],
          "dynamic_simple": "rendinst_simple",
          "simple_aces": "rendinst_simple",
          "dynamic_simple_colored": "rendinst_simple_colored",
          "dynamic_tree": "rendinst_tree",
          "dynamic_tree_colored": "rendinst_tree_colored",
          "dynamic_alpha_blend": "rendinst_simple",
          "dynamic_facing_leaves": "rendinst_facing_leaves",
          "dynamic_glass_chrome": "rendinst_glass_chrome",
          "dynamic_masked_chrome_bump": "rendinst_simple",
          "dynamic_perlin_layered": "rendinst_perlin_layered"
        }
      },
      "dynModel": {
        "descListOutPath": "dynModelDesc",
        "separateModelMatToDescBin": true,
        "ignoreMappingInPrepareBillboardMesh": true,
        "enableMeshNodeCollapse": false,
        "maxBonesCount": 256,
        "setBonePerVertex": 4,
        "maxVPRConst": 1024,
        "useDirectBones": true,
        "remapShaders": {
          "bat_shader": "bat_shader",
          "rendinst_simple": "dynamic_masked_chrome_bump",
          "rendinst_simple_glass": "dynamic_simple_glass",
          "glass": "dynamic_simple_glass",
          "glass_refraction": "dynamic_refraction",
          "simple": "dynamic_simple",
          "dynamic_null": "dynamic_null",
          "dynamic_masked_chrome": "dynamic_masked_chrome",
          "dynamic_illum": "dynamic_illum",
          "aces_weapon_fire": "aces_weapon_fire",
          "dynamic_tele": "scope_lens",
          "land_mesh_combined": "dynamic_land_mesh_combined",
          "land_mesh_combined_2x": "dynamic_land_mesh_combined_2x",
          "random_grass": "random_grass",
          "tracer_head": "tracer_head",
          "gi_black": "",
          "cockpit_shader": "cockpit_shader",
          "collimator": "collimator",
          "dynamic_ablend_hair": "dynamic_ablend_hair",
          "dynamic_aircraft": "dynamic_aircraft",
          "dynamic_alpha_blend": "dynamic_alpha_blend",
          "dynamic_bakeanim_emissive_metatex": "dynamic_bakeanim_emissive_metatex",
          "dynamic_bakeanim_metatex": "dynamic_bakeanim_metatex",
          "dynamic_bakeanim_refraction_metatex": "dynamic_bakeanim_refraction_metatex",
          "dynamic_bakeanim_translucent_metatex": "dynamic_bakeanim_translucent_metatex",
          "dynamic_cloth": "dynamic_cloth",
          "dynamic_collimator": "dynamic_collimator",
          "dynamic_crosshair": "dynamic_crosshair",
          "dynamic_emissive": "dynamic_emissive",
          "dynamic_emissive_zombie": "dynamic_emissive_zombie",
          "dynamic_emissive_metatex": "dynamic_emissive_metatex",
          "dynamic_eye": "dynamic_eye",
          "dynamic_glass_chrome": "dynamic_glass_chrome",
          "dynamic_hair": "dynamic_hair",
          "dynamic_highlighter": "dynamic_highlighter",
          "dynamic_mask_layered": "dynamic_mask_layered",
          "dynamic_masked_chrome_bump": "dynamic_masked_chrome_bump",
          "dynamic_masked_chrome_bump_colored": "dynamic_masked_chrome_bump_colored",
          "dynamic_masked_glass_chrome": "dynamic_masked_glass_chrome",
          "dynamic_masked_ship": "dynamic_masked_ship",
          "dynamic_masked_tank": "dynamic_masked_tank",
