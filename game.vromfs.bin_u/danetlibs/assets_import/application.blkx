          "propeller_front": "propeller_front",
          "propeller_side": "propeller_side",
          "scope_lens": "scope_lens",
          "dynamic_chevron": "dynamic_chevron",
          "denyShaders": {
            "regExp": "^rendinst_"
          }
        }
      }
    }
  }
}