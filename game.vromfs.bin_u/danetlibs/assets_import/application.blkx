          "dynamic_mirror": "dynamic_mirror",
          "dynamic_painted_by_mask": "dynamic_painted_by_mask",
          "dynamic_perlin_layered": "dynamic_perlin_layered",
          "dynamic_refraction": "dynamic_refraction",
          "dynamic_refraction_metatex": "dynamic_refraction_metatex",
          "dynamic_semi_trans": "dynamic_semi_trans",
          "dynamic_simple": "dynamic_simple",
          "dynamic_simple_glass": "dynamic_simple_glass",
          "dynamic_simple_painted": "dynamic_simple_painted",
          "dynamic_simple_transparent": "dynamic_simple_transparent",
          "dynamic_skin": "dynamic_skin",
          "dynamic_spline_attachment": "dynamic_spline_attachment",
          "dynamic_spline_gen": "dynamic_spline_gen",
          "dynamic_emissive_spline_gen": "dynamic_emissive_spline_gen",
