          "text": "#mainmenu/2step/getGP",
          "link": "#url/2step/codeApp",
          "on_click": "onMsgLink",
          "btnName": "X",
          "ButtonImg": []
        }
      },
      "Button_text": {
        "pos": "pw-w-2@blockInterval, ph-h-2@blockInterval",
        "position": "absolute",
        "visualStyle": "noFrame",
        "isLink": "yes",
        "isFeatured": "yes",
        "link": "#url/2step/signInTroubles",
        "on_click": "onMsgLink",
        "btnName": "R3",
        "btnText": {
          "text": "#mainmenu/2step/getGP/whyNeed",
          "underline": []
        },
        "ButtonImg": []
      },
      "CheckBox": {
        "width": "0.5pw",
        "pos": "2@blockInterval, ph-h-2@blockInterval",
        "position": "absolute",
        "text": "#options/dont_show_again",
        "on_change_value": "onDontShowChange",
        "btnName": "Y",
        "ButtonImg": [],
        "CheckBoxImg": []
      }
    }
  }
}