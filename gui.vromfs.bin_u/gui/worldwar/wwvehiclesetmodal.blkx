{
  "root": {
    "blur": {},
    "blur_foreground": {},
    "frame": {
      "class": "wnd",
      "type": "dark",
      "height": "1@maxWindowHeightNoSrh",
      "pos": "0.5pw-0.5w, 1@minYposWindow",
      "position": "absolute",
      "flow": "h-flow",
      "wwBattleBackgroundBlock": {
        "wwBattleBackground": {
          "id": "operation_background"
        }
      },
      "tdiv": [
        {
          "size": "pw, ph",
          "position": "absolute",
          "wwBattleBackgroundShadow": [
            {
              "type": "left"
            },
            {
              "type": "right"
            },
            {
              "type": "top"
            },
            {
              "type": "bottom",
              "top": "ph-h"
            }
          ]
        },
        {
          "height": "fh",
          "flow": "horizontal",
          "scrollbarShortcuts": "yes",
          "tdiv": [
            {
              "id": "team_SIDE_1_unit_info",
              "height": "ph",
              "left": "0",
              "position": "relative"
            },
            {
              "id": "team_SIDE_2_unit_info",
              "height": "ph",
              "left": "1@slotbarWidth-w",
              "position": "absolute"
            }
          ]
        },
        {
          "right": "0",
          "position": "relative",
          "activeText": {
            "id": "cant_join_reason_txt",
            "top": "0.5ph-0.5h",
            "position": "relative",
            "padding-right": "1@blockInterval",
            "text": ""
          },
          "cardImg": {
            "id": "warning_icon",
            "padding-right": "1@blockInterval",
            "display": "hide",
            "background-image": "#ui/gameuiskin#btn_help.svg",
            "tooltip": ""
          },
          "Button_text": {
            "id": "btn_auto_preset",
            "text": "#worldwar/btnAutoPreset",
            "_on_click": "onRunAutoPreset",
            "btnName": "R3",
            "display": "hide",
            "enable": "no",
            "ButtonImg": {},
            "warningIcon": {
              "id": "auto_preset_warning_icon",
              "type": "warning",
              "tooltip": "#generatePreset/warning/can_create_best_preset/tooltip",
              "display": "hide"
            }
          }
        },
        {
          "id": "nav-slotbar",
          "pos": "0.5pw-0.5w, 1@slotbarTop+1@blockInterval",
          "position": "relative"
        }
      ],
      "frame_header": {
        "activeText": {
          "id": "wnd_title",
          "caption": "yes"
        },
        "Button_close": {}
      }
    }
  }
}