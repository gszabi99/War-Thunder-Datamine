[
  {
    "tdiv": {
      "id": "bg_picture_container",
      "size": "sw, sh",
      "position": "root",
      "background-color": "@black",
      "tdiv": [
        {
          "pos": "50%sw-50%w,50%sh-50%h"
        },
        {
          "position": "absolute"
        },
        {
          "size": "2.333sh, sh"
        },
        {
          "overflow": "hidden"
        },
        {
          "tdiv": {
            "id": "animated_bg_picture",
            "pos": "pw/2-w/2, ph/2-h/2",
            "position": "absolute",
            "size": "2.5sh, sh",
            "behaviour": "multiLayerImage",
            "blend-time": "500"
          }
        },
        {
          "tdiv": {
            "size": "0.333sh, sh",
            "pos": "-w/2, 0",
            "position": "absolute",
            "background-image": "!ui/gameuiskin#debriefing_bg_grad@@ss",
            "background-color": "@black"
          }
        },
        {
          "tdiv": {
            "size": "0.333sh, sh",
            "pos": "pw -w/2, 0",
            "position": "absolute",
            "background-image": "!ui/gameuiskin#debriefing_bg_grad@@ss",
            "background-color": "@black"
          }
        }
      ]
    }
  },
  {
    "tdiv": {
      "width": "0.67@rw $min 2@rh",
      "pos": "50%pw-50%w,sh-1@bh-h-20@dp",
      "position": "absolute",
      "background-color": "@shadeBackgroundColor4",
      "background-repeat": "expand",
      "padding": "0.005sw,0.01sh,0.005sw,0.01sh",
      "textAreaCentered": {
        "text": "#preloaderSettings/hint",
        "style": "color:@helpF1Color;",
        "pos": "50%pw-50%w,0",
        "position": "relative",
        "width": "pw"
      }
    }
  },
  {
    "tdiv": {
      "pos": "@bw+30@dp,@bh+30@dp",
      "size": "sw,sh",
      "frame": {
        "behaviour": "moveObj",
        "class": "wndNav",
        "height": "1@maxWindowHeight",
        "total-input-transparent": "yes",
        "frame_header": {
          "activeText": {
            "text": "#preloaderSettings/title",
            "caption": "yes"
          },
          "Button_close": []
        },
        "div": {
          "size": "pw,fh",
          "tdiv": {
            "size": "pw,ph",
            "mislist": "yes",
            "chapterListPlace": [
              {
                "id": "chapter_place"
              },
              {
                "height": "ph"
              },
              {
                "flow": "vertical"
              },
              {
                "tdiv": {
                  "id": "div_before_chapters_list",
                  "width": "pw",
                  "display": "hide"
                }
              },
              {
                "tdiv": {
                  "id": "chapters_list_place",
                  "size": "pw, fh",
                  "flow": "vertical",
                  "tdiv": {
                    "id": "listbox_filter_holder",
                    "width": "pw",
                    "pos": "1@blockInterval, 0",
                    "position": "relative"
                  },
                  "listbox": {
                    "id": "items_list",
                    "size": "pw, fh",
                    "flow": "vertical",
                    "move-only-hover": "yes",
                    "navigatorShortcuts": "yes",
                    "on_select": "onItemSelect",
                    "_on_dbl_click": "onItemDblClick"
                  },
                  "textAreaCentered": {
                    "id": "items_list_msg",
                    "width": "80%pw",
                    "pos": "50%pw-50%w, 40%ph-40%h",
                    "position": "absolute",
                    "hideEmptyText": "yes",
                    "text": ""
                  },
                  "animated_wait_icon": {
                    "id": "items_list_wait_icon",
                    "pos": "50%pw-50%w, 40%ph-40%h",
                    "position": "absolute",
                    "background-rotation": "0",
                    "display": "hide"
                  }
                }
              }
            ]
          }
        },
        "navBar": {
          "navRight": [
            {
              "Button_text": {
                "id": "btn_select",
                "text": "#mainmenu/btnSelect",
                "btnName": "A",
                "ButtonImg": []
              }
            },
            {
              "Button_text": {
                "id": "btn_ban",
                "text": "#maps/preferences/ban",
                "btnName": "X",
                "_on_click": "toggleBan",
                "ButtonImg": []
              }
            }
          ]
        }
      }
    }
  }
]