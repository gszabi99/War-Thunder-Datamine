{
  "id": "options-root",
  "root": {
    "bgrStyle": "fullScreenWnd",
    "blur": [],
    "blur_foreground": [],
    "frame": {
      "id": "wnd_frame",
      "width": "@rw",
      "height": "sh-@bottomMenuPanelHeight",
      "pos": "0.5pw-0.5w, 0.5ph-0.5h-0.5@bottomMenuPanelHeight",
      "position": "absolute",
      "class": "wndNav",
      "fullScreenSize": "yes",
      "frame_header": {
        "Breadcrumb": {
          "Button_text": {
            "_on_click": "goBack",
            "visualStyle": "noBgr",
            "img": [],
            "btnText": {
              "text": "#mainmenu/menu"
            },
            "text": {
              "text": " | "
            },
            "textareaNoTab": {
              "text": "#mainmenu/btnSessionSettings"
            }
          }
        },
        "Button_close": []
      },
      "HorizontalListBox": {
        "id": "groups_list",
        "position": "relative",
        "pos": "0, 0.5sh-0.5@rh-@blockInterval",
        "height": "1@frameHeaderHeight",
        "class": "header",
        "normalFont": "yes",
        "activeAccesskeys": "RS",
        "on_select": "onGroupSelect",
        "display": "hide"
      },
      "rowSeparator": {
        "top": "-1@dp"
      },
      "tdiv": {
        "size": "pw, fh",
        "padding": "0, 2@blockInterval, 0, 1@blockInterval+1@dp",
        "flow": "horizontal",
        "div": {
          "id": "control_navigation",
          "height": "ph"
        },
        "container": {
          "height": "ph",
          "width": "fw",
          "flow": "vertical",
          "div": {
            "id": "search_container",
            "width": "pw-0.15@sf",
            "padding": "2@blockInterval",
            "pos": "50%pw-50%w, 0",
            "position": "relative",
            "display": "hide",
            "EditBox": {
              "id": "filter_edit_box",
              "pos": "0, ph/2-h/2",
              "position": "relative",
              "width": "fw",
              "on_activate": "onFilterEditBoxActivate",
              "on_change_value": "onFilterEditBoxChangeValue",
              "on_cancel_edit": "onFilterEditBoxCancel",
              "text": "",
              "edit-hint": "#contacts/search_placeholder"
            },
            "Button_text": {
              "id": "filter_edit_cancel_btn",
              "pos": "0, ph/2-h/2",
              "position": "relative",
              "class": "image",
              "showConsoleImage": "no",
              "noMargin": "yes",
              "tooltip": "#mainmenu/btnReset",
              "on_click": "onFilterEditBoxCancel",
              "display": "hide",
              "img": {
                "background-image": "#ui/gameuiskin#btn_close.svg"
              }
            }
          },
          "optionsBoxRightScroll": {
            "id": "optionslist"
          }
        }
      },
      "navBar": []
    },
    "gamercard_div": {
      "id": "opt_gamercard",
      "short": "yes",
      "gamercard": {
        "id": "gamercard_bottom",
        "systemMsg": {
          "pos": "1@systemMsgIndent, 0",
          "position": "root",
          "flow": "vertical",
          "dlDataStat": {
            "behavior": "bhvUpdater",
            "pos": "50%pw-50%w,0",
            "position": "relative",
            "value": "{\"viewId\": \"DL_DATA_STATE_TEXT\"}",
            "css-hier-invalidate": "yes",
            "animText": {
              "text": "",
              "font": "@fontTiny",
              "color-factor": "0",
              "fade": "out"
            }
          }
        },
        "BottomMenuPanel": [
          {
            "shadowSafeArea": []
          },
          {
            "tdiv": {
              "size": "pw, 0.02@sf",
              "pos": "0, ph",
              "position": "absolute",
              "background-repeat": "expand",
              "background-image": "#ui/gameuiskin#optionlist_background_top",
              "background-color": "#90909090",
              "background-position": "0, 0.25h, 0, 0"
            }
          },
          {
            "tdiv": [
              {
                "width": "1@bottomGcWidth"
              },
              {
                "height": "ph"
              },
              {
                "pos": "pw/2 - w/2, ph/2 - h/2"
              },
              {
                "position": "absolute"
              },
              {
                "tdiv": {
                  "id": "gamercard_bottom_left",
                  "height": "1@gamercardHeight",
                  "class": "header",
                  "width": "fw",
                  "bottomNavPlace": {
                    "id": "gamercard_bottom_navbar_place",
                    "size": "pw - ph, ph",
                    "pos": "0, 0.5(ph-h)",
                    "position": "absolute"
                  },
                  "tdiv": {
                    "id": "slotbar-presetsPlace",
                    "size": "pw, ph",
                    "position": "absolute",
                    "display": "hide"
                  }
                }
              },
              {
                "voiceChatWidgetNest": [
                  {
                    "isClanOnly": "no"
                  },
                  {
                    "blockSeparator": {
                      "display": "hide"
                    }
                  },
                  {
                    "emptyVoiceChat": {
                      "tooltip": "#clan/voice_chat/widget",
                      "usertalk": {
                        "id": "voice_chat_no_activity",
                        "type": "noTalk",
                        "fade": "in",
                        "height-end": "1",
                        "_size-timer": "0",
                        "img": {
                          "color-factor": "255",
                          "inacive": "yes",
                          "position": "relative"
                        },
                        "activeText": {
                          "text": "...",
                          "color-factor": "255",
                          "position": "relative"
                        }
                      }
                    }
                  },
                  {
                    "voiceChatWidgetContainer": {
                      "isSmall": "no",
                      "voiceChatWidget": {
                        "value": "{\"viewId\": \"VOICE_CHAT\"}"
                      }
                    }
                  },
                  {
                    "blockSeparator": {
                      "display": "hide"
                    }
                  }
                ]
              },
              {
                "gcButtonsHolder": {
                  "id": "gamercard_squad_widget",
                  "position": "relative",
                  "top": "0.5(ph-h)",
                  "margin-right": "2@buttonTextPadding",
                  "class": "header"
                }
              },
              {
                "gcButtonsHolder": [
                  {
                    "id": "gamercard_bottom_right"
                  },
                  {
                    "position": "relative"
                  },
                  {
                    "top": "0.5(ph-h)"
                  },
                  {
                    "class": "header"
                  },
                  {
                    "Button_text": [
                      {
                        "id": "gc_invites_btn"
                      },
                      {
                        "class": "image"
                      },
                      {
                        "tooltip": "#mainmenu/invites"
                      },
                      {
                        "_on_click": "onGC_invites"
                      },
                      {
                        "img": {
                          "id": "icon",
                          "background-image": "#ui/gameuiskin#invites.svg"
                        }
                      },
                      {
                        "img": {
                          "id": "iconNew",
                          "background-image": "#ui/gameuiskin#invites_new.svg",
                          "style": "background-color:@textureGlowColor",
                          "display": "hide"
                        }
                      },
                      {
                        "img": {
                          "id": "iconGlow",
                          "background-image": "#ui/gameuiskin#mail_new_glow",
                          "style": "background-color:@textureGlowColor; size:110%ph, 110%ph",
                          "_transp-timer": "0",
                          "display": "hide"
                        }
                      }
                    ]
                  },
                  {
                    "Button_text": {
                      "id": "gc_contacts",
                      "class": "image",
                      "tooltip": "#mainmenu/contacts",
                      "_on_click": "onGC_contacts",
                      "img": {
                        "background-image": "#ui/gameuiskin#friends.svg"
                      },
                      "text": {
                        "id": "gc_friends_online",
                        "position": "absolute",
                        "pos": "pw - w, ph - h - 2",
                        "style": "font:@fontTiny; color:@userlogColoredText",
                        "shadeStyle": "outline33pct",
                        "text": ""
                      }
                    }
                  },
                  {
                    "Button_text": [
                      {
                        "id": "gc_chat_btn"
                      },
                      {
                        "class": "image"
                      },
                      {
                        "tooltip": "#mainmenu/chat"
                      },
                      {
                        "_on_click": "onGC_chat"
                      },
                      {
                        "img": {
                          "id": "icon",
                          "background-image": "#ui/gameuiskin#chat.svg"
                        }
                      },
                      {
                        "img": {
                          "id": "iconNew",
                          "background-image": "#ui/gameuiskin#chat_new.svg",
                          "style": "background-color:@textureGlowColor",
                          "display": "hide"
                        }
                      },
                      {
                        "img": {
                          "id": "iconGlow",
                          "background-image": "#ui/gameuiskin#mail_new_glow",
                          "style": "background-color:@textureGlowColor; size:110%ph, 110%ph",
                          "_transp-timer": "0",
                          "display": "hide"
                        }
                      },
                      {
                        "text": {
                          "id": "gc_new_chat_messages",
                          "position": "absolute",
                          "pos": "pw - w, ph - h",
                          "style": "font:@fontTiny; color:@userlogColoredText",
                          "text": ""
                        }
                      }
                    ]
                  },
                  {
                    "Button_text": [
                      {
                        "id": "gc_userlog_btn"
                      },
                      {
                        "class": "image"
                      },
                      {
                        "tooltip": "#userlog/no_new_messages"
                      },
                      {
                        "_on_click": "onUserLog"
                      },
                      {
                        "img": {
                          "id": "icon",
                          "background-image": "#ui/gameuiskin#mail.svg"
                        }
                      },
                      {
                        "img": {
                          "id": "iconNew",
                          "background-image": "#ui/gameuiskin#mail_new.svg",
                          "style": "background-color:@textureGlowColor",
                          "display": "hide"
                        }
                      },
                      {
                        "img": {
                          "id": "iconGlow",
                          "background-image": "#ui/gameuiskin#mail_new_glow",
                          "style": "background-color:@textureGlowColor; size:110%ph, 110%ph",
                          "_transp-timer": "0",
                          "display": "hide"
                        }
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ],
        "tdiv": {
          "position": "root",
          "pos": "sw - w, 0",
          "versionText": []
        }
      }
    }
  }
}