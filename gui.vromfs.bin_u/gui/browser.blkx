{
  "root": {
    "type": "big",
    "blur": {},
    "blur_foreground": {},
    "frame": {
      "id": "wnd_frame",
      "size": "1@rw, 1@rh",
      "pos": "50%pw-50%w, 50%ph-50%h",
      "position": "absolute",
      "class": "wndNav",
      "frame_header": {
        "activeText": {
          "id": "wnd_title",
          "caption": "yes",
          "text": "#mainmenu/btnBrowser",
          "textHide": "yes",
          "pare-text": "yes",
          "width": "fw"
        },
        "Button_close": {
          "id": "btn_back",
          "style": "_on_click='browserCloseAndUpdateEntitlements'"
        }
      },
      "browser_area": {
        "id": "browser_area",
        "re-type": "9rect",
        "behaviour": "browser",
        "url": "",
        "size": "pw, ph",
        "on_embedded_browser_event": "embedded_browser_event"
      },
      "animated_wait_icon": {
        "id": "browserWaitAnimation",
        "pos": "50%pw-50%w,50%ph-50%h",
        "position": "absolute",
        "background-rotation": "0"
      },
      "navBar": {
        "navLeft": {
          "Button_text": {
            "id": "btn_browser_force_external",
            "text": "#browser/open_external",
            "_on_click": "browserForceExternal",
            "btnName": "LB",
            "ButtonImg": {}
          }
        },
        "navRight": {
          "Button_text": [
            {
              "id": "btn_browser_back",
              "text": "#mainmenu/btnBack",
              "_on_click": "browserGoBack",
              "btnName": "X",
              "ButtonImg": {}
            },
            {
              "id": "btn_browser_reload",
              "text": "#mainmenu/btnRefresh",
              "_on_click": "onBrowserBtnReload",
              "btnName": "Y",
              "ButtonImg": {}
            },
            {
              "id": "btn_back",
              "text": "#mainmenu/btnQuit",
              "style": "_on_click='browserCloseAndUpdateEntitlements'",
              "btnName": "B",
              "ButtonImg": {}
            }
          ]
        }
      }
    }
  },
  "tdiv": {
    "position": "root",
    "pos": "@bw +1@framePadding, sh-h -1@blockInterval -@bh -1@frameFooterHeight",
    "voiceChatWidgetNest": {
      "isClanOnly": "no",
      "blockSeparator": [
        {
          "display": "hide"
        },
        {
          "display": "hide"
        }
      ],
      "emptyVoiceChat": {
        "tooltip": "#clan/voice_chat/widget",
        "usertalk": {
          "id": "voice_chat_no_activity",
          "type": "noTalk",
          "fade": "in",
          "height-end": "1",
          "_size-timer": "0",
          "img": {
            "color-factor": "255",
            "inacive": "yes",
            "position": "relative"
          },
          "activeText": {
            "text": "...",
            "color-factor": "255",
            "position": "relative"
          }
        }
      },
      "voiceChatWidgetContainer": {
        "isSmall": "no",
        "voiceChatWidget": {
          "value": "{\"viewId\": \"VOICE_CHAT\"}"
        }
      }
    }
  }
}