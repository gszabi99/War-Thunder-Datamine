{
  "tdiv": {
    "id": "hud_hints_nest",
    "width": "2/3@rwHud",
    "flow": "vertical",
    "css-hier-invalidate": "yes",
    "behavior": "Timer",
    "tdiv": [
      {
        "id": "ship_obstacle_rf",
        "size": "pw, 0.04@shHud"
      },
      {
        "width": "pw",
        "position": "relative",
        "pos": "pw/2 - w/2, 0",
        "flow": "vertical",
        "tdiv": [
          {
            "id": "common_priority_hints_holder",
            "position": "relative",
            "tdiv": {
              "id": "common_priority_hints",
              "width": "pw",
              "position": "relative",
              "pos": "0, ph-h",
              "flow": "vertical"
            }
          },
          {
            "width": "pw",
            "tdiv": {
              "id": "mission_hints",
              "width": "pw",
              "position": "relative",
              "pos": "0, ph-h",
              "flow": "vertical"
            }
          },
          {
            "id": "mission_action_hints_holder",
            "position": "relative",
            "tdiv": {
              "id": "mission_action_hints",
              "width": "pw",
              "position": "relative",
              "pos": "0, ph-h",
              "flow": "vertical"
            }
          }
        ]
      },
      {
        "position": "relative",
        "left": "pw/2 - w/2",
        "div": [
          {
            "id": "iconed_hints"
          },
          {
            "id": "display_timers"
          }
        ]
      },
      {
        "id": "actionbar_hints_nest",
        "width": "1@shHud",
        "left": "0.5pw-0.5w",
        "position": "relative",
        "tdiv": {
          "width": "pw",
          "position": "relative",
          "pos": "0, ph-h",
          "flow": "vertical",
          "tdiv": {
            "id": "actionbar_hints",
            "width": "pw",
            "flow": "vertical"
          }
        }
      },
      {
        "id": "tutorial_hints",
        "width": "pw",
        "position": "root",
        "pos": "50%sw-50%w, 1@bhHud + 0.67@shHud",
        "flow": "vertical"
      },
      {
        "id": "minor_priority_hints",
        "width": "pw",
        "position": "root",
        "pos": "50%sw-50%w, @shHud - h",
        "flow": "vertical"
      }
    ],
    "hudMessages": [
      {
        "id": "hud_message_zone_capture_notification",
        "width": "pw",
        "message_style": "zone_capture_notification",
        "input-transparent": "yes"
      },
      {
        "id": "hud_message_player_damage_notification",
        "message_style": "player_damage_notification",
        "left": "0.2@shHud",
        "position": "relative",
        "flow": "vertical",
        "width": "pw - 0.2@shHud"
      }
    ],
    "textAreaCentered": {
      "id": "low-quality-model-warning",
      "width": "pw",
      "style": "color:@warningTextColor; font:@fontHudNormal;",
      "text": "#low_quality_model_in_hangar",
      "display": "hide"
    }
  }
}