{
  "_size-timer": "1",
  "title": "$tooltipObj",
  "awardDiv": {
    "id": "move_part",
    "size": "ph, ph",
    "pos": "(pw-w)/2, (ph-h)/2",
    "position": "absolute",
    "noMargin": "yes",
    "css-hier-invalidate": "yes",
    "input-transparent": "yes",
    "total-input-transparent": "yes",
    "layeredIconContainer": {
      "id": "award_img"
    },
    "img": {
      "id": "award_multiplier",
      "background-image": "#ui/gameuiskin#window_body_shadow",
      "pos": "pw-w, ph-h + 0.2ph",
      "position": "relative",
      "display": "hide",
      "textarea": {
        "id": "amount_text",
        "overlayTextColor": "userlog",
        "text": "",
        "removeParagraphIndent": "yes",
        "padding": "0.05ph"
      }
    }
  },
  "tooltipObj": {
    "id": "tooltip_",
    "display": "hide",
    "on_tooltip_open": "onAwardTooltipOpen",
    "on_tooltip_close": "onTooltipObjClose"
  }
}