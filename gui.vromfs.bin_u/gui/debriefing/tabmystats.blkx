{
  "debriefingTab": {
    "id": "my_stats_tab",
    "flow": "vertical",
    "overflow-y": "auto",
    "tdiv": {
      "size": "pw, fh",
      "flow": "horizontal",
      "tdiv": {
        "size": "fw, ph",
        "flow": "vertical",
        "debrBlock": {
          "id": "top_bar_nest",
          "width": "pw",
          "pos": "0, 1@debrMyPlaceBlockOffset",
          "position": "relative",
          "padding": "1@debrPad, 0.5@debrPad",
          "background-color": "@separatorBlockColor",
          "tdiv": [
            {
              "id": "my_place_move_box",
              "pos": "0, 0.5ph-0.5h",
              "position": "relative",
              "css-hier-invalidate": "yes",
              "total-input-transparent": "yes",
              "display": "hide",
              "activeText": [
                {
                  "id": "my_place_label",
                  "mediumFont": "yes",
                  "animateFontSize": "yes",
                  "text": "#debriefing/placeInMyTeam"
                },
                {
                  "id": "my_place_in_mptable",
                  "mediumFont": "yes",
                  "animateFontSize": "yes",
                  "overlayTextColor": "userlog",
                  "text": "0"
                }
              ]
            },
            {
              "id": "my_stats_top_bar",
              "top": "0.5ph-0.5h",
              "position": "relative",
              "tdiv": [
                {
                  "id": "wnd_total",
                  "top": "0.5ph-0.5h",
                  "position": "relative"
                },
                {
                  "id": "buy_premium",
                  "pos": "1@debrPad, 0.5ph-0.5h",
                  "position": "relative",
                  "display": "hide",
                  "Button_text": {
                    "id": "btn_buy_premium_award",
                    "class": "buyPremAward",
                    "style": "margin-right:0;",
                    "on_click": "onBuyPremiumAward",
                    "visualStyle": "purchase",
                    "tooltip": "",
                    "btnName": "Y",
                    "buttonWink": {
                      "_transp-timer": "0"
                    },
                    "buttonGlance": {},
                    "ButtonImg": {},
                    "textarea": {
                      "id": "label",
                      "pos": "0, 0.5ph-0.5h",
                      "position": "relative",
                      "min-width": "pw",
                      "input-transparent": "yes",
                      "text-align": "center",
                      "overlayTextColor": "premiumNotEarned",
                      "text": ""
                    }
                  },
                  "textareaNoTab": {
                    "max-width": "150@sf/@pf",
                    "pos": "0.3@sIco, 0.5ph-0.5h",
                    "position": "relative",
                    "overlayTextColor": "userlog",
                    "smallFont": "yes",
                    "text": "#mainmenu/notEarnedPremiumReward"
                  }
                },
                {
                  "id": "active_wager_container",
                  "pos": "1@debrPad, 0.5ph-0.5h",
                  "position": "relative",
                  "title": "$tooltipObj",
                  "display": "hide",
                  "smallFont": "yes",
                  "interactive": "yes",
                  "css-hier-invalidate": "yes",
                  "tdiv": {
                    "size": "70%@debrWagerHeight, 1@debrWagerHeight",
                    "itemIconBlock": {
                      "id": "active_wager_icon",
                      "pos": "0.5pw-0.5w, 0.5ph-0.5h",
                      "position": "absolute",
                      "debriefingSize": "yes",
                      "overflow": "hidden"
                    },
                    "img": {
                      "id": "active_wager_result_icon",
                      "size": "0.5ph, 0.5ph",
                      "pos": "pw-0.7w, ph-h",
                      "position": "absolute",
                      "background-image": "",
                      "background-svg-size": "0.5ph, 0.5ph",
                      "background-repeat": "aspect-ratio"
                    }
                  },
                  "textareaNoTab": {
                    "id": "active_wager_text",
                    "pos": "10@sf/@pf, 0.5ph-0.5h",
                    "position": "relative",
                    "overlayTextColor": "userlog",
                    "smallFont": "yes",
                    "text": ""
                  },
                  "focus_border": {},
                  "tooltipObj": {
                    "id": "active_wager_tooltip",
                    "on_tooltip_open": "onWagerTooltipOpen",
                    "on_tooltip_close": "onTooltipObjClose",
                    "display": "hide",
                    "enable": "no"
                  }
                }
              ],
              "debriefingBonusIconNest": {
                "id": "bonus_ico_place",
                "size": "1@debrBonusWidth, 1@debrBonusHeight",
                "pos": "1@debrPad, 0.5ph-0.5h",
                "position": "relative",
                "display": "hide",
                "interactive": "yes",
                "css-hier-invalidate": "yes",
                "img": {
                  "id": "bonus_ico",
                  "size": "1@debrBonusWidth, 1@debrBonusHeight"
                },
                "tdiv": {
                  "id": "start_bonus_place",
                  "size": "2.5@debrBonusWidth, 2.5@debrBonusHeight",
                  "pos": "0.5pw-0.5w, 0.5ph-0.5h",
                  "position": "absolute"
                },
                "focus_border": {
                  "class": "expanded"
                }
              }
            }
          ]
        },
        "tdiv": {
          "size": "pw, fh",
          "tdiv": [
            {
              "id": "left_block",
              "min-width": "1@navBarBattleButtonMinWidth",
              "size": "2@debriefingUnlockIconSize+1@debrPad, ph",
              "margin-right": "@debrBigGap",
              "flow": "vertical",
              "debrBlock": [
                {
                  "id": "inventory_gift_block",
                  "width": "pw",
                  "padding": "0, 1@debrPad",
                  "display": "hide",
                  "tdiv": {
                    "width": "pw",
                    "flow": "vertical",
                    "div": {
                      "width": "pw",
                      "flow": "h-flow",
                      "flow-align": "center",
                      "tdiv": {
                        "id": "inventory_gift_icon",
                        "width": "pw",
                        "flow": "h-flow",
                        "flow-align": "center",
                        "smallItems": "no"
                      },
                      "Button_text": {
                        "id": "btn_show_all",
                        "pos": "0.5pw-0.5w",
                        "margin-bottom": "0",
                        "on_click": "onViewRewards",
                        "text": "#mainmenu/showMore",
                        "display": "hide",
                        "enable": "no",
                        "btnName": "R3",
                        "ButtonImg": {}
                      }
                    },
                    "Button_text": {
                      "id": "btn_inventory_gift_action",
                      "pos": "0.5pw-0.5w, 0",
                      "position": "relative",
                      "max-width": "1@buttonWidth",
                      "margin-top": "1@buttonMargin",
                      "class": "battle",
                      "navButtonFont": "yes",
                      "btnName": "LT",
                      "css-hier-invalidate": "yes",
                      "_on_click": "onInventoryGiftAction",
                      "display": "hide",
                      "enable": "no",
                      "pattern": {},
                      "buttonWink": {
                        "_transp-timer": "0"
                      },
                      "buttonGlance": {},
                      "ButtonImg": {},
                      "btnText": {
                        "id": "btn_inventory_gift_action_text"
                      }
                    }
                  },
                  "debrSeparator": {
                    "class": "bottom"
                  }
                },
                {
                  "id": "my_stats_awards_block",
                  "size": "pw, fh",
                  "class": "button",
                  "behaviour": "button",
                  "on_click": "onViewAwards",
                  "total-input-transparent": "yes",
                  "skip-navigation": "yes",
                  "margin-top": "1@blockInterval",
                  "tdiv": {
                    "size": "pw, ph",
                    "pos": "0.5pw-0.5w, 0",
                    "position": "relative",
                    "flow": "vertical",
                    "tdiv": [
                      {
                        "id": "awards_list_challenges",
                        "pos": "0.5pw - 0.5w, 0",
                        "position": "relative",
                        "size": "pw, 1@debriefingUnlockIconSize",
                        "overflow": "visible",
                        "display": "hide",
                        "behavior": "PosNavigator",
                        "navigatorShortcuts": "active",
                        "on_activate": "onViewAwards"
                      },
                      {
                        "size": "pw, fh",
                        "pos": "0.5pw-0.5w, 0",
                        "position": "relative",
                        "flow": "horizontal",
                        "tdiv": [
                          {
                            "id": "awards_list_unlocks",
                            "pos": "pw/9, 0",
                            "size": "1@debriefingUnlockIconSize, ph",
                            "position": "absolute",
                            "overflow": "visible",
                            "flow": "vertical",
                            "display": "hide",
                            "behavior": "PosNavigator",
                            "navigatorShortcuts": "active",
                            "on_activate": "onViewAwards"
                          },
                          {
                            "id": "awards_list_streaks",
                            "pos": "5pw/9, 0",
                            "size": "1@debriefingUnlockIconSize, ph",
                            "position": "absolute",
                            "overflow": "visible",
                            "flow": "vertical",
                            "display": "hide",
                            "behavior": "PosNavigator",
                            "navigatorShortcuts": "active",
                            "on_activate": "onViewAwards"
                          }
                        ]
                      }
                    ],
                    "div": {
                      "id": "btn_challenge_div",
                      "width": "pw",
                      "pos": "pw/2 - w/2, 0",
                      "display": "hide",
                      "flow": "vertical",
                      "margin": "0, 1@blockInterval",
                      "Button_text": {
                        "id": "btn_challenge",
                        "navButtonFont": "yes",
                        "class": "battle",
                        "css-hier-invalidate": "yes",
                        "_on_click": "onGotoChallenge",
                        "pattern": {
                          "type": "bright_texture"
                        },
                        "buttonWink": {
                          "_transp-timer": "0"
                        },
                        "buttonGlance": {},
                        "ButtonImg": {},
                        "btnText": {
                          "id": "btn_next_text",
                          "text": "#mainmenu/btnUnlockChallenge"
                        }
                      },
                      "rowSeparator": {
                        "margin-top": "1@blockInterval"
                      }
                    }
                  }
                }
              ]
            },
            {
              "size": "fw, ph",
              "flow": "vertical",
              "debrBlock": [
                {
                  "id": "pve_trophy_block",
                  "size": "pw, 0.75@itemHeight",
                  "padding": "0, 1@debrPad",
                  "display": "hide",
                  "tdiv": [
                    {
                      "id": "pve_trophy_progress",
                      "pos": "0, 0.5ph-0.5h",
                      "position": "relative",
                      "width": "75%pw"
                    },
                    {
                      "id": "pve_trophy_chest",
                      "size": "0.75@itemHeight, 0.75@itemHeight",
                      "pos": "pw-w, 0.5ph-0.5h",
                      "position": "absolute"
                    },
                    {
                      "size": "0.75@itemHeight, 0.75@itemHeight",
                      "pos": "pw-w-0.25w, 0.5ph-0.5h",
                      "position": "absolute",
                      "tdiv": {
                        "id": "pve_trophy_content",
                        "size": "1@itemHeight, 1@itemHeight",
                        "pos": "0.5pw-0.5w, 0.5ph-0.5h",
                        "position": "absolute"
                      }
                    }
                  ],
                  "activeText": {
                    "id": "pve_award_already_received",
                    "pos": "pw-w-1@debrPad, 50%ph",
                    "position": "absolute",
                    "text": "#debriefing/awardHasAlreadyBeenReceived",
                    "display": "hide"
                  },
                  "debrSeparator": {
                    "class": "bottom"
                  }
                },
                {
                  "id": "no_awards_block",
                  "size": "pw, 0.12@scrn_tgt",
                  "padding": "0, 1@debrPad",
                  "display": "hide",
                  "tdiv": {
                    "width": "pw",
                    "pos": "0, 0.5ph-0.5h",
                    "position": "relative",
                    "flow": "vertical",
                    "textareaNoTab": {
                      "id": "no_awards_caption",
                      "width": "pw",
                      "position": "relative",
                      "overlayTextColor": "userlog",
                      "text": ""
                    },
                    "Button_text": {
                      "id": "btn_no_awards_info",
                      "position": "relative",
                      "on_click": "onNoAwardsInfoBtn",
                      "noMargin": "yes",
                      "text": "",
                      "display": "hide",
                      "enable": "no",
                      "btnName": "L3",
                      "externalLink": "yes",
                      "activeText": {
                        "text": "#msgbox/btn_more",
                        "css-hier-invalidate": "yes",
                        "underline": {}
                      },
                      "ButtonImg": {}
                    }
                  },
                  "debrSeparator": {
                    "class": "bottom"
                  }
                }
              ],
              "tdiv": {
                "size": "pw, fh",
                "debrBlock": {
                  "id": "my_stats_scroll_block",
                  "size": "fw, ph",
                  "flow": "vertical",
                  "overflow-y": "auto",
                  "scrollbarShortcuts": "yes",
                  "css-hier-invalidate": "yes",
                  "tdiv": {
                    "width": "pw",
                    "min-height": "ph",
                    "margin": "0, 1@debrPad",
                    "flow": "vertical",
                    "table": {
                      "id": "stat_table",
                      "width": "pw",
                      "baseRow": "rows16",
                      "flow": "vertical",
                      "total-input-transparent": "yes",
                      "behavior": "PosNavigator",
                      "navigatorShortcuts": "noSelect",
                      "selfFocusBorder": "yes"
                    },
                    "tdiv": {
                      "id": "leaderbord_stats",
                      "width": "pw",
                      "pos": "0, 1@debrPad",
                      "position": "relative",
                      "flow": "h-flow",
                      "display": "hide"
                    },
                    "textareaNoTab": {
                      "id": "stat_info_text",
                      "width": "pw",
                      "pos": "0, 3@debrPad",
                      "position": "relative",
                      "overlayTextColor": "",
                      "text-align": "right",
                      "text": "",
                      "display": "hide"
                    }
                  }
                }
              }
            }
          ]
        }
      },
      "debrBlock": {
        "id": "right_block",
        "size": "2@slot_width+1@scrollBarSize, ph",
        "pos": "0, 1@debrMyPlaceBlockOffset",
        "position": "relative",
        "margin-left": "@debrBigGap",
        "flow": "vertical",
        "display": "hide",
        "debrBlock": {
          "id": "battle_tasks_block",
          "width": "pw",
          "max-height": "0.5ph",
          "margin-bottom": "1@debrPad",
          "flow": "vertical",
          "tdiv": {
            "size": "pw, 1@buttonHeight",
            "background-color": "@separatorBlockColor",
            "activeText": {
              "width": "pw",
              "pos": "1@debrPad, 0.5ph-0.5h",
              "position": "relative",
              "text": "#userlog/page/battletasks"
            }
          },
          "listbox": {
            "id": "current_battle_tasks",
            "width": "pw",
            "padding-left": "1@debrPad",
            "overflow-y": "auto",
            "flow": "vertical",
            "scrollbarShortcuts": "left",
            "background-color": "@frameDarkTransparentBgColor"
          }
        },
        "tdiv": {
          "id": "researches_scroll_block",
          "size": "pw, fh",
          "overflow-y": "auto",
          "scrollbarShortcuts": "left",
          "tdiv": {
            "width": "pw",
            "min-height": "ph",
            "flow": "vertical",
            "tdiv": [
              {
                "size": "pw, 1@buttonHeight",
                "background-color": "@separatorBlockColor",
                "activeText": {
                  "width": "pw",
                  "pos": "1@debrPad, 0.5ph-0.5h",
                  "position": "relative",
                  "text": "#debriefing/researched_unit"
                }
              },
              {
                "width": "pw",
                "margin-bottom": "1@debrPad",
                "background-color": "@frameDarkTransparentBgColor",
                "textAreaCentered": [
                  {
                    "id": "no_air_text",
                    "width": "pw",
                    "pos": "0.5pw-0.5w, 0.5ph-0.5h",
                    "position": "absolute",
                    "smallFont": "yes",
                    "text": ""
                  },
                  {
                    "id": "air_item_text",
                    "width": "pw",
                    "pos": "0.5pw-0.5w, 0.5ph-0.5h",
                    "position": "absolute",
                    "smallFont": "yes",
                    "text": ""
                  }
                ],
                "rankUpList": {
                  "id": "air_item_place",
                  "max-width": "pw-2@debrPad",
                  "min-height": "@slot_height+2@slot_vert_pad",
                  "position": "relative",
                  "pos": "0.5pw-0.5w, 0",
                  "flow": "h-flow",
                  "holdTooltipChildren": "yes"
                }
              },
              {
                "size": "pw, 1@buttonHeight",
                "background-color": "@separatorBlockColor",
                "activeText": {
                  "width": "pw",
                  "pos": "1@debrPad, 0.5ph-0.5h",
                  "position": "relative",
                  "text": "#debriefing/research_list"
                }
              },
              {
                "id": "research_list",
                "width": "pw",
                "flow": "vertical",
                "background-color": "@frameDarkTransparentBgColor",
                "textAreaCentered": {
                  "id": "research_list_text",
                  "width": "pw",
                  "pos": "0.5pw-0.5w, 0.5(@slot_height + 2@slot_vert_pad)-0.5h",
                  "position": "absolute",
                  "smallFont": "yes",
                  "text": ""
                },
                "tdiv": {
                  "id": "modifications_objects_place",
                  "width": "pw",
                  "min-height": "1@slot_height + 2@slot_vert_pad",
                  "margin-left": "1@debrPad",
                  "flow": "vertical",
                  "interactiveChildren": "yes"
                }
              }
            ]
          }
        }
      }
    }
  }
}