{
  "title": "$tooltipObj",
  "td": [
    {
      "width": "0.5pw",
      "min-width": "0.4@sf",
      "img": {
        "id": "name_icon",
        "size": "1@sIco, 1@sIco",
        "top": "0.5ph-0.5h",
        "position": "relative",
        "background-image": "",
        "background-svg-size": "1@sIco, 1@sIco"
      },
      "activeText": {
        "id": "name"
      }
    },
    {
      "id": "td_value",
      "min-width": "0.1@sf",
      "cellType": "tdRight",
      "activeText": {
        "id": "value",
        "parseTags": "yes"
      },
      "img": {
        "id": "value_image",
        "size": "1@sIco, 1@sIco",
        "top": "0.5ph-0.5h",
        "position": "relative",
        "background-image": "",
        "background-svg-size": "1@sIco, 1@sIco"
      }
    },
    {
      "id": "td_reward",
      "min-width": "0.1@sf",
      "cellType": "tdRight",
      "activeText": {
        "id": "reward",
        "parseTags": "yes"
      },
      "img": {
        "id": "reward_image",
        "size": "1@sIco, 1@sIco",
        "top": "0.5ph-0.5h",
        "position": "relative",
        "background-image": "",
        "background-svg-size": "1@sIco, 1@sIco"
      }
    }
  ],
  "tooltipObj": {
    "id": "tooltip_",
    "display": "hide",
    "on_tooltip_open": "onTrTooltipOpen",
    "on_tooltip_close": "onTooltipObjClose"
  }
}