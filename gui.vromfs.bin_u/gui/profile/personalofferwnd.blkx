{
  "root": {
    "type": "big",
    "blur": {},
    "blur_foreground": {},
    "tdiv": {
      "pos": "0, 0.5ph-0.5h",
      "position": "absolute",
      "size": "1@swOrRwInVr, 1@shOrRhInVr",
      "background-color": "#CC111821",
      "flow": "vertical",
      "img": {
        "id": "offer_image",
        "width": "pw",
        "height": "0.211w",
        "max-height": "0.45ph",
        "background-image": "!ui/images/offer/personal_offer_image",
        "background-repeat": "aspect-ratio",
        "tdiv": {
          "pos": "0.5pw - 0.5w, 0.6ph - 0.5h",
          "position": "absolute",
          "flow": "horizontal",
          "img": [
            {
              "width": "365@sf/@pf",
              "height": "242@sf/@pf",
              "background-image": "ui/images/offer/personal_offer_header",
              "background-position": "0, 0, 500, 0",
              "background-repeat": "part"
            },
            {
              "id": "header_image_center",
              "height": "242@sf/@pf",
              "background-image": "ui/images/offer/personal_offer_header",
              "background-position": "450, 0, 450, 0",
              "background-repeat": "part"
            },
            {
              "width": "365@sf/@pf",
              "height": "242@sf/@pf",
              "background-image": "ui/images/offer/personal_offer_header",
              "background-position": "500, 0, 0, 0",
              "background-repeat": "part"
            }
          ],
          "textareaNoTab": [
            {
              "margin-left": "160@sf/@pf",
              "margin-top": "64@sf/@pf",
              "position": "absolute",
              "text": "#specialOffer/limited",
              "mediumFont": "yes",
              "overlayTextColor": "active"
            },
            {
              "id": "personal_text",
              "margin-left": "160@sf/@pf",
              "margin-top": "100@sf/@pf",
              "position": "absolute",
              "text": "#specialOffer/personal",
              "bigBoldFont": "yes",
              "overlayTextColor": "active"
            },
            {
              "id": "time_expired_text",
              "margin-left": "pw - w - 120@sf/@pf",
              "margin-top": "46@sf/@pf - h",
              "position": "absolute",
              "text": "#specialOffer/valid",
              "smallFont": "yes",
              "overlayTextColor": "active"
            },
            {
              "id": "time_expired_value",
              "margin-left": "pw - 152@sf/@pf - w/2",
              "margin-top": "0.44ph - h/2",
              "position": "absolute",
              "mediumFont": "yes",
              "overlayTextColor": "gray",
              "css-hier-invalidate": "yes"
            }
          ]
        }
      },
      "tdiv": [
        {
          "id": "offer_markup",
          "pos": "pw/2 - w/2, 0",
          "position": "relative",
          "flow": "horizontal"
        },
        {
          "width": "1@rw",
          "pos": "0.5pw-0.5w, 1@bh",
          "position": "absolute",
          "Breadcrumb": {
            "normalFont": "yes",
            "Button_text": {
              "on_click": "goBack",
              "visualStyle": "noBgr",
              "img": {},
              "btnText": {
                "text": "#mainmenu/hangar"
              }
            }
          },
          "tdiv": {
            "position": "absolute",
            "pos": "pw-w-1@buttonCloseHeight, 0",
            "Button_text": [
              {
                "id": "gc_warpoints",
                "visualStyle": "noFrame",
                "tooltip": "#mainmenu/warpoints",
                "showBonusPersonal": "",
                "showBonusCommon": "",
                "_on_click": "onOnlineShopLions",
                "img": {
                  "isFirstLeft": "yes",
                  "size": "@cIco, @cIco",
                  "background-image": "#ui/gameuiskin#shop_warpoints.svg",
                  "background-svg-size": "@cIco, @cIco"
                },
                "btnText": {
                  "id": "gc_balance",
                  "min-width": "0.05@sf",
                  "pos": "1@blockInterval, 50%ph-50%h",
                  "position": "relative",
                  "text-align": "left"
                },
                "BonusCorner": [
                  {
                    "bonusType": "personal"
                  },
                  {
                    "bonusType": "common"
                  }
                ],
                "chapterSeparator": {
                  "position": "absolute",
                  "pos": "pw, 0"
                }
              },
              {
                "id": "gc_eagles",
                "visualStyle": "noFrame",
                "tooltip": "#mainmenu/gold",
                "_on_click": "onOnlineShopEagles",
                "img": {
                  "isFirstLeft": "yes",
                  "size": "@cIco, @cIco",
                  "background-image": "#ui/gameuiskin#shop_warpoints_premium.svg",
                  "background-svg-size": "@cIco, @cIco"
                },
                "btnText": {
                  "id": "gc_gold",
                  "min-width": "0.05@sf",
                  "pos": "1@blockInterval, 50%ph-50%h",
                  "position": "relative",
                  "text-align": "left"
                },
                "chapterSeparator": {
                  "position": "absolute",
                  "pos": "pw, 0"
                }
              }
            ]
          },
          "Button_close": {
            "id": "btn_close",
            "on_click": "goBack"
          }
        }
      ],
      "Button_text": {
        "id": "btn_buy",
        "size": "389@sf/@pf, 54@sf/@pf",
        "pos": "pw/2 - w/2, 0",
        "position": "relative",
        "margin-top": "60@sf/@pf",
        "hideText": "yes",
        "on_click": "onBuy",
        "btnName": "X",
        "visualStyle": "purchase",
        "buttonWink": {},
        "buttonGlance": {},
        "tdiv": {
          "id": "btn_buy_text_block",
          "pos": "0.5pw-0.5w, 0.5ph-0.5h",
          "position": "relative"
        },
        "ButtonImg": {}
      }
    },
    "gamercard_div": {}
  },
  "timer": {
    "id": "update_timer",
    "timer_handler_func": "onTimer",
    "timer_interval_msec": "1000"
  }
}