{
  "root": {
    "bgrStyle": "fullScreenWnd",
    "blur_foreground": {
      "filledDark": "yes"
    },
    "tdiv": [
      {
        "position": "root",
        "width": "sw",
        "height": "@maxAccountHeaderHeight",
        "id": "profile_header_picture",
        "overflow-y": "auto",
        "img": {
          "position": "absolute",
          "width": "sw*2",
          "height": "@maxAccountHeaderHeight",
          "pos": "(pw-w)/2, 0",
          "background-color": "#FFFFFFFF",
          "background-image": "!ui/images/profile/account_profile_header_bg",
          "background-repeat": "aspect-ratio"
        }
      },
      {
        "position": "absolute",
        "size": "sw, 200@sf/@pf",
        "rotation": "180",
        "background-image": "!ui/images/profile/wnd_gradient.svg",
        "background-color": "#402F4453"
      }
    ],
    "frame": {
      "id": "wnd_frame",
      "width": "1@rw",
      "height": "1@rh",
      "pos": "0.5pw-0.5w, (ph-h)/2",
      "max-width": "1@maxProfileFrameWidth",
      "position": "absolute",
      "class": "wndNav",
      "profilePage": "yes",
      "fullScreenSize": "yes",
      "needShortSeparators": "yes",
      "css-hier-invalidate": "yes",
      "frame_header": {
        "smallSize": "yes",
        "top": "10@sf/@pf",
        "Breadcrumb": {
          "normalFont": "yes",
          "Button_text": {
            "_on_click": "goBack",
            "visualStyle": "noBgr",
            "img": {},
            "btnText": {
              "id": "back_scene_name"
            },
            "text": {
              "text": " | "
            },
            "textareaNoTab": {
              "id": "breadcrumb_title",
              "text": "#mainmenu/btnProfile"
            }
          }
        },
        "Button_close": {
          "id": "btn_back"
        }
      },
      "tdiv": [
        {
          "position": "relative",
          "width": "pw",
          "css-hier-invalidate": "yes",
          "min-height": "1@frameHeaderHeight",
          "frameSeparator": {
            "position": "absolute",
            "top": "ph-h",
            "left": "(pw-w)/2"
          },
          "HorizontalListBox": {
            "id": "profile_sheet_list",
            "height": "1@frameHeaderHeight",
            "class": "header",
            "activeAccesskeys": "RS",
            "normalFont": "yes",
            "on_select": "onSheetChange"
          }
        },
        {
          "size": "pw, fh",
          "flow": "vertical",
          "tdiv": {
            "id": "profile_header",
            "flow": "vertical",
            "position": "relative",
            "pos": "(pw-w)/2, 0",
            "padding": "28@sf/@pf",
            "css-hier-invalidate": "yes",
            "frameSeparator": {
              "position": "root",
              "top": "1@maxAccountHeaderHeight"
            },
            "tdiv": {
              "size": "pw, ph",
              "pos": "50%pw-50%w, 0",
              "position": "relative",
              "css-hier-invalidate": "yes",
              "Button_text": {
                "id": "btn_profile_icon",
                "class": "profileImg",
                "_on_click": "onChangePilotIcon",
                "div": {
                  "id": "profile-icon",
                  "size": "pw,ph",
                  "behavior": "bhvAvatar",
                  "isFull": "yes",
                  "border": "yes",
                  "border-color": "#FFFFFF",
                  "border-radius": "2@sf/@pf",
                  "css-hier-invalidate": "yes",
                  "profileEditBtn": {
                    "type": "big",
                    "pos": "pw - w - 5@sf/@pf, 5@sf/@pf",
                    "_on_click": "onChangePilotIcon"
                  }
                },
                "ButtonImg": {},
                "unseenIcon": {
                  "id": "unseen_avatar",
                  "pos": "4@dp, 4@dp",
                  "position": "absolute",
                  "unseenText": {}
                }
              },
              "tdiv": {
                "position": "relative",
                "height": "1@avatarButtonSize",
                "left": "23@sf/@pf",
                "css-hier-invalidate": "yes",
                "tdiv": [
                  {
                    "width": "pw",
                    "position": "absolute",
                    "flow": "vertical",
                    "css-hier-invalidate": "yes",
                    "tdiv": [
                      {
                        "auto-scroll": "slow",
                        "tdiv": [
                          {
                            "re-type": "textarea",
                            "behaviour": "textArea",
                            "id": "profile-clanTag",
                            "max-width": "pw",
                            "font": "@fontBigBold",
                            "tooltip": "",
                            "padding-right": "1@blockInterval",
                            "hideEmptyText": "yes",
                            "text": "",
                            "color": "@mainPlayerColor"
                          },
                          {
                            "position": "relative",
                            "re-type": "textarea",
                            "behaviour": "textArea",
                            "id": "profile-name",
                            "talign": "left",
                            "font": "@fontBigBold",
                            "style": "color:@mainPlayerColor"
                          }
                        ]
                      },
                      {
                        "position": "relative",
                        "css-hier-invalidate": "yes",
                        "Button_text": {
                          "id": "profile_currentUser_btn_title",
                          "position": "relative",
                          "left": "-1@buttonTextPadding",
                          "top": "4@sf/@pf",
                          "hideEmptyText": "yes",
                          "isProfileTitleBtn": "yes",
                          "css-hier-invalidate": "yes",
                          "noMargin": "yes",
                          "visualStyle": "noBgr",
                          "on_click": "openChooseTitleWnd",
                          "textareaNoTab": {
                            "id": "profile-currentUser-title",
                            "pos": "0, 0.5ph-0.5h",
                            "position": "relative",
                            "unseenIcon": {
                              "id": "unseen_titles",
                              "pos": "pw + 1@blockInterval, 50%ph-50%h",
                              "position": "absolute",
                              "unseenText": {}
                            }
                          },
                          "focus_border": {},
                          "ButtonImg": {}
                        },
                        "profileEditBtn": {
                          "pos": "pw,(ph - h)/2",
                          "_on_click": "openChooseTitleWnd"
                        }
                      }
                    ],
                    "textareaNoTab": [
                      {
                        "id": "profile-currentUser-psnName",
                        "width": "pw",
                        "smallFont": "yes"
                      },
                      {
                        "id": "profile-currentUser-steamName",
                        "width": "pw",
                        "text": "",
                        "smallFont": "yes"
                      }
                    ]
                  },
                  {
                    "id": "level_progress_container",
                    "position": "relative",
                    "flow": "horizontal",
                    "width": "pw",
                    "top": "ph-h-11@sf/@pf",
                    "tdiv": [
                      {
                        "position": "relative",
                        "size": "42@sf/@pf, 42@sf/@pf",
                        "pos": "0, ph/2-h/2",
                        "cardImg": {
                          "id": "profile-prestige",
                          "pos": "(pw-w)/2, (ph-h)/2",
                          "type": "profile",
                          "position": "absolute"
                        }
                      },
                      {
                        "position": "relative",
                        "size": "2@sf/@pf, 42@sf/@pf",
                        "top": "(ph-h)/2",
                        "background-color": "#6f767d",
                        "margin-right": "14@sf/@pf",
                        "margin-left": "14@sf/@pf"
                      },
                      {
                        "position": "relative",
                        "flow": "vertical",
                        "width": "308@sf/@pf",
                        "pos": "0, (ph - h)/2 - @sf/@pf",
                        "tdiv": {
                          "position": "relative",
                          "pos": "0, 0",
                          "padding-bottom": "8@sf/@pf",
                          "width": "pw",
                          "tdiv": {
                            "position": "relative",
                            "flow": "horizontal",
                            "pos": "pw-w-2@sf/@pf, 0",
                            "activeText": [
                              {
                                "smallFont": "yes",
                                "text": "#mainmenu/rank",
                                "position": "relative"
                              },
                              {
                                "id": "profile-rank",
                                "smallFont": "yes",
                                "position": "relative",
                                "padding": "2@sf/@pf, 0"
                              }
                            ]
                          },
                          "activeText": {
                            "id": "profile-prestige_title",
                            "smallFont": "yes",
                            "position": "absolute",
                            "left": "2@sf/@pf"
                          }
                        },
                        "expProgress": {
                          "padding-top": "",
                          "id": "profile-rankProgress",
                          "width": "pw",
                          "position": "relative",
                          "value": "0",
                          "isForProfile": "yes"
                        }
                      }
                    ]
                  }
                ]
              }
            }
          },
          "profilePage": {
            "id": "pages_container",
            "size": "pw, fh + 3@sf/@pf",
            "flow": "vertical",
            "input-transparent": "yes",
            "padding-top": "6@sf/@pf",
            "tdiv": [
              {
                "position": "root",
                "size": "sw, 410@sf/@pf",
                "max-width": "1@maxProfileFrameWidth",
                "pos": "(sw-w)/2, sh - h",
                "background-image": "!ui/images/profile/smoke_bg",
                "background-color": "#FFFFFF"
              },
              {
                "id": "usercard-container",
                "size": "pw, fh",
                "flow": "vertical",
                "tdiv": {
                  "position": "absolute",
                  "left": "(pw-w)/2",
                  "size": "@accountHeaderWidth, (sh - @frameFooterHeight - @maxAccountHeaderHeight) $min 924@sf/@pf",
                  "background-image": "!ui/images/profile/widget_bg",
                  "background-color": "#FFFFFF"
                }
              },
              {
                "id": "stats-container",
                "size": "pw, fh",
                "padding-top": "4@blockInterval",
                "total-input-transparent": "yes",
                "display": "hide",
                "tdiv": [
                  {
                    "position": "absolute",
                    "left": "(pw-w)/2",
                    "size": "@accountHeaderWidth, (sh - @frameFooterHeight - @maxAccountHeaderHeight) $min 924@sf/@pf",
                    "background-image": "!ui/images/profile/widget_bg",
                    "background-color": "#FFFFFF"
                  },
                  {
                    "position": "absolute",
                    "size": "@accountHeaderWidth, ph",
                    "left": "(pw-w)/2",
                    "flow": "vertical",
                    "padding-top": "24@sf/@pf",
                    "tdiv": {
                      "size": "pw, ph",
                      "flow": "vertical",
                      "total-input-transparent": "yes",
                      "tdiv": [
                        {
                          "position": "relative",
                          "size": "pw, 48@sf/@pf",
                          "background-color": "#11111111",
                          "HorizontalListBox": {
                            "id": "modes_list",
                            "height": "ph",
                            "width": "pw",
                            "left": "(pw-w)/2",
                            "position": "relative",
                            "navigatorShortcuts": "yes",
                            "move-only-hover": "yes",
                            "autoSize": "yes",
                            "bottomSelectLine": "yes",
                            "transparentButtons": "yes",
                            "buttonsHasPadding": "yes",
                            "on_select": "onProfileStatsModeChange"
                          }
                        },
                        {
                          "size": "pw + 20@sf/@pf, fh",
                          "overflow-y": "auto",
                          "scrollbarShortcuts": "yes",
                          "table": {
                            "id": "stats_table",
                            "class": "lbTable",
                            "width": "@accountHeaderWidth",
                            "padding": "27@sf/@pf, 27@sf/@pf, 27@sf/@pf, 7@sf/@pf",
                            "isProfileStatsTable": "yes",
                            "text-halign": "center"
                          }
                        },
                        {
                          "position": "absolute",
                          "top": "48@sf/@pf",
                          "size": "pw, 54@sf/@pf",
                          "background-image": "#!ui/images/profile/wnd_gradient.svg",
                          "background-color": "#FF111922",
                          "background-repeat": "expand-svg",
                          "rotation": "180"
                        },
                        {
                          "position": "absolute",
                          "top": "ph-h",
                          "size": "pw, 52@sf/@pf",
                          "background-image": "#!ui/images/profile/wnd_gradient.svg",
                          "background-color": "#FF111922",
                          "background-repeat": "expand-svg"
                        }
                      ]
                    }
                  }
                ],
                "animated_wait_icon": {
                  "id": "profile_loading",
                  "pos": "0.5(pw-w),0.4(ph-h)",
                  "position": "absolute",
                  "background-rotation": "0"
                },
                "timer": {
                  "id": "profile_update",
                  "timer_handler_func": "onUpdate"
                }
              }
            ],
            "profileContent": [
              {
                "id": "records-container",
                "flow": "vertical",
                "padding-top": "4@blockInterval",
                "total-input-transparent": "yes",
                "display": "hide",
                "tdiv": {
                  "id": "filters_div",
                  "flow": "horizontal",
                  "width": "pw",
                  "position": "relative",
                  "padding-right": "450@sf/@pf",
                  "bgcolor": "#11080808",
                  "margin-bottom": "1@blockInterval",
                  "HorizontalListBox": {
                    "id": "modes_list",
                    "fullTransparent": "yes",
                    "bottomSelectLine": "yes",
                    "navigatorShortcuts": "yes",
                    "move-only-hover": "yes",
                    "on_select": "onStatsModeChange"
                  },
                  "tdiv": {
                    "width": "450@sf/@pf",
                    "left": "pw-w",
                    "position": "absolute",
                    "flow": "horizontal",
                    "EditBox": {
                      "id": "filter_edit_box",
                      "width": "fw",
                      "on_change_value": "applyFilter",
                      "on_cancel_edit": "onFilterCancel",
                      "text": "",
                      "edit-hint": "#contacts/search_placeholder"
                    },
                    "popupFilter": {
                      "margin-left": "1@blockInterval"
                    }
                  }
                },
                "table": {
                  "id": "airs_stats_table",
                  "width": "pw",
                  "height": "fh",
                  "pos": "0.5pw-0.5w, 0",
                  "position": "relative",
                  "class": "lbTable",
                  "higlightRowOnHover": "yes",
                  "text-valign": "center"
                },
                "animated_wait_icon": {
                  "id": "stats_loading",
                  "pos": "50%pw-50%w, 50%ph-50%h",
                  "position": "absolute",
                  "background-rotation": "0"
                }
              },
              {
                "id": "medals-container",
                "padding-top": "3@blockInterval",
                "flow": "horizontal",
                "display": "hide",
                "profileContentLeft": {
                  "listbox": {
                    "id": "medals_list",
                    "size": "pw, ph",
                    "position": "relative",
                    "flow-align": "left",
                    "isBigSizeList": "yes",
                    "navigator": "posNavigator",
                    "moveX": "linear",
                    "moveY": "closest",
                    "navigatorShortcuts": "yes",
                    "move-only-hover": "yes",
                    "on_select": "onMedalsCountrySelect"
                  }
                },
                "profileContentSeparator": {},
                "profileContentRight": {
                  "size": "@profilePageRightPartWidth, ph",
                  "position": "relative",
                  "flow": "vertical",
                  "tdiv": [
                    {
                      "id": "medals_info",
                      "position": "relative",
                      "flow": "vertical",
                      "overflow-y": "auto",
                      "total-input-transparent": "yes",
                      "width": "pw",
                      "tdiv": {
                        "id": "medals_desc",
                        "flow": "horizontal",
                        "width": "pw",
                        "margin-top": "16@sf/@pf",
                        "margin-bottom": "1@profilePagePartsMargin"
                      }
                    },
                    {
                      "position": "relative",
                      "size": "pw, 2@sf/@pf",
                      "background-color": "#4B4F53",
                      "margin": "0, 6@sf/@pf, 0, 19@sf/@pf"
                    }
                  ],
                  "medalsList": {
                    "medalsListContent": {
                      "id": "medals_zone",
                      "on_select": "onMedalSelect"
                    }
                  }
                }
              }
            ],
            "frameSeparator": {
              "position": "absolute",
              "top": "ph",
              "left": "(pw-w)/2"
            }
          }
        }
      ],
      "navBar": {
        "min-height": "10@sf/@pf + 1@frameFooterHeight",
        "navRight": {
          "Button_text": [
            {
              "id": "btn_friendChangeNick",
              "text": "#mainmenu/addCustomNick",
              "btnName": "RT",
              "on_click": "onFriendChangeNick",
              "ButtonImg": {}
            },
            {
              "id": "btn_friendAdd",
              "text": "#contacts/friendlist/add",
              "on_click": "onFriendAdd",
              "btnName": "X",
              "ButtonImg": {}
            },
            {
              "id": "btn_friendRemove",
              "text": "#contacts/friendlist/remove",
              "on_click": "onFriendRemove",
              "btnName": "X",
              "ButtonImg": {}
            },
            {
              "id": "btn_blacklistAdd",
              "text": "#contacts/blacklist/add",
              "on_click": "onBlacklistAdd",
              "btnName": "Y",
              "ButtonImg": {}
            },
            {
              "id": "btn_blacklistRemove",
              "text": "#contacts/blacklist/remove",
              "on_click": "onBlacklistRemove",
              "btnName": "Y",
              "ButtonImg": {}
            },
            {
              "id": "btn_moderatorBan",
              "text": "#contacts/moderator_ban",
              "on_click": "onBlacklistBan",
              "btnName": "L3",
              "ButtonImg": {}
            },
            {
              "id": "btn_complain",
              "text": "#mainmenu/btnComplain",
              "btnName": "RB",
              "_on_click": "onComplain",
              "ButtonImg": {}
            },
            {
              "id": "btn_xbox_profile",
              "text": "#mainmenu/btnXboxProfile",
              "btnName": "LB",
              "on_click": "onOpenXboxProfile",
              "display": "hide",
              "ButtonImg": {}
            },
            {
              "id": "btn_psn_profile",
              "text": "#mainmenu/btnPsnProfile",
              "btnName": "LB",
              "on_click": "onOpenPSNProfile",
              "display": "hide",
              "ButtonImg": {}
            },
            {
              "id": "btn_leaderboard",
              "text": "#mainmenu/btnLeaderboards",
              "btnName": "RB",
              "on_click": "onLeaderboard",
              "display": "hide",
              "enable": "no",
              "ButtonImg": {}
            },
            {
              "id": "btn_achievements_url",
              "text": "#mainmenu/compareAchievements",
              "btnName": "R3",
              "on_click": "onOpenAchievementsUrl",
              "display": "hide",
              "externalLink": "yes",
              "hideText": "yes",
              "ButtonImg": {},
              "btnText": {
                "id": "btn_achievements_url_text",
                "text": "#mainmenu/compareAchievements",
                "underline": {}
              }
            }
          ]
        },
        "navMiddle": {
          "id": "paginator_place"
        }
      }
    }
  }
}