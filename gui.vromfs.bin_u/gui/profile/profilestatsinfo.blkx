{
  "flow": "vertical",
  "tdiv": [
    {
      "width": "pw",
      "flow": "horizontal",
      "padding-bottom": "1@blockInterval",
      "HorizontalListBox": {
        "id": "modes_list",
        "type": "transparent",
        "navigatorShortcuts": "yes",
        "move-only-hover": "yes",
        "on_select": "onStatsModeChange"
      },
      "tdiv": {
        "width": "450@sf/@pf",
        "left": "pw-w",
        "position": "absolute",
        "flow": "horizontal",
        "EditBox": {
          "id": "filter_edit_box",
          "width": "fw",
          "on_change_value": "applyFilter",
          "on_cancel_edit": "onFilterCancel",
          "text": "",
          "edit-hint": "#contacts/search_placeholder"
        },
        "popupFilter": {
          "margin-left": "1@blockInterval"
        }
      }
    },
    {
      "size": "pw, fh",
      "position": [
        "relative",
        "relative"
      ],
      "pos": "0.5pw-0.5w, 0",
      "table": {
        "id": "airs_stats_table",
        "size": "pw, ph",
        "pos": "0.5pw-0.5w, 0",
        "position": "relative",
        "class": "lbTable",
        "higlightRowOnHover": "yes",
        "text-valign": "center",
        "text-halign": "center"
      }
    }
  ],
  "animated_wait_icon": {
    "id": "stats_loading",
    "pos": "50%pw-50%w, 50%ph-50%h",
    "position": "absolute",
    "background-rotation": "0"
  }
}