{
  "SlotbarBackground": {
    "id": "slotbar_background",
    "size": "pw, ph",
    "display": "hide",
    "pos": "50%pw-50%w, 0",
    "position": "absolute",
    "hasPresetsPanel": "no",
    "tdiv": {
      "size": "pw, ph",
      "overflow": "hidden",
      "blur": {},
      "blur_foreground": {}
    },
    "blockSeparator": {},
    "slotbarTop": {
      "id": "slotbar_buttons_place",
      "size": "pw, 1@slotbarTop",
      "pos": "0,-h",
      "position": "absolute",
      "display": "hide",
      "behavior": "Timer",
      "tdiv": [
        {
          "id": "header_countries_nest",
          "height": "ph",
          "css-hier-invalidate": "yes",
          "DummyButton": [
            {
              "id": "prev_country_btn",
              "padding": "1@blockInterval, 0",
              "btnName": "LB",
              "on_click": "prevCountry",
              "ButtonImg": {
                "class": "independent",
                "fullSizeIcons": "yes"
              }
            },
            {
              "id": "next_country_btn",
              "padding": "1@blockInterval, 0",
              "btnName": "RB",
              "on_click": "nextCountry",
              "ButtonImg": {
                "class": "independent",
                "fullSizeIcons": "yes"
              }
            }
          ],
          "activateSelect": {
            "id": "header_countries",
            "height": "ph",
            "class": "slotsHeader",
            "normalFont": "yes",
            "isShort": "no",
            "css-hier-invalidate": "yes",
            "total-input-transparent": "yes",
            "navigatorShortcuts": "yes",
            "on_select": "onHeaderCountry",
            "on_dbl_click": "onCountriesListDblClick"
          }
        },
        {
          "id": "autorefill-settings",
          "left": "pw-w",
          "top": "50%(ph-h)",
          "position": "absolute",
          "padding-left": "3@dp",
          "display": "hide",
          "CheckBox": [
            {
              "id": "slots-autorepair",
              "margin-right": "0.5h",
              "class": "with_image",
              "tooltip": "#shop/auto_repair",
              "on_change_value": "onSlotsChangeAutoRefill",
              "infoImg": {
                "size": "@cIco, @cIco",
                "background-svg-size": "@cIco, @cIco",
                "background-image": "#ui/gameuiskin#slot_repair.svg"
              },
              "CheckBoxImg": {}
            },
            {
              "id": "slots-autoweapon",
              "margin-right": "0.5h",
              "class": "with_image",
              "tooltip": "#shop/auto_buy_weapons",
              "on_change_value": "onSlotsChangeAutoRefill",
              "infoImg": {
                "size": "@cIco, @cIco",
                "background-svg-size": "@cIco, @cIco",
                "background-image": "#ui/gameuiskin#btn_weapons.svg"
              },
              "CheckBoxImg": {}
            }
          ]
        }
      ]
    }
  },
  "CountriesCrews": {
    "id": "countries_crews",
    "needHugeFooter": "no"
  },
  "animShow": {
    "id": "slotbar_shade",
    "size": "pw, 1@slotbarHeight + 1@slotbarOffset + 1@slotbarTop -1@bh",
    "pos": "50%pw-50%w, -1@slotbarTop",
    "position": "absolute",
    "background-color": "@shadeBackgroundColor",
    "animation": "hide",
    "color-factor": "0"
  },
  "timer": {
    "id": "slotbar_timer"
  }
}