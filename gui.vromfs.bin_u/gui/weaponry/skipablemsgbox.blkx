{
  "root": {
    "blur": {},
    "blur_foreground": {},
    "frame": {
      "width": "0.75@scrn_tgt+10",
      "pos": "50%pw-50%w, 45%ph-40%h",
      "position": "absolute",
      "class": "wnd",
      "frame_header": {
        "Button_close": {},
        "top_right_holder": {
          "hasRightIndent": "yes",
          "CheckBox": {
            "id": "skip_this",
            "pos": "0, 50%ph-50%h",
            "position": "relative",
            "text": "#options/doNotAskMeAgain",
            "on_change_value": "onSkipMessageBox",
            "btnName": "Y",
            "CheckBoxImg": {},
            "ButtonImg": {}
          }
        }
      },
      "textAreaCentered": [
        {
          "id": "msgText",
          "width": "pw",
          "text": ""
        },
        {
          "id": "listText",
          "width": "pw",
          "text": "",
          "margin-top": "2*@sf/100.0"
        }
      ],
      "tdiv": {
        "id": "buttons",
        "pos": "50%pw-50%w, 0.03@sf",
        "position": "relative",
        "behavior": "posNavigator",
        "navigatorShortcuts": "noSelect",
        "total-input-transparent": "yes",
        "Button_text": [
          {
            "id": "btn_cancel",
            "text": "",
            "btnName": "B",
            "on_click": "goBack",
            "showConsoleImage": "no"
          },
          {
            "id": "btn_select",
            "text": "#mainmenu/toBattle",
            "on_click": "onStart",
            "showConsoleImage": "no"
          }
        ]
      },
      "CheckBox": {
        "id": "slots-autoweapon",
        "pos": "50%pw-50%w, 0.01@sf",
        "position": "relative",
        "class": "with_image",
        "text": "#shop/auto_buy_weapons",
        "smallFont": "yes",
        "display": "hide",
        "on_change_value": "onSlotsChangeAutoRefill",
        "infoImg": {
          "size": "@cIco, @cIco",
          "background-svg-size": "@cIco, @cIco",
          "background-image": "#ui/gameuiskin#btn_weapons.svg"
        },
        "CheckBoxImg": {}
      }
    }
  },
  "dummy": {
    "behavior": "button",
    "on_click": "onAcceptSelection",
    "btnName": "A"
  }
}