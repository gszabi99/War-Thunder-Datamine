{
  "root": {
    "behaviour": "button",
    "class": "button",
    "blur": {},
    "blur_foreground": {},
    "frame": {
      "id": "wnd_frame",
      "size": "1.2@sf, 1@maxWindowHeight - 0.1@sh",
      "class": "wndNav",
      "isCenteredUnderLogo": "yes",
      "css-hier-invalidate": "yes",
      "frame_header": {
        "css-hier-invalidate": "yes",
        "activeText": {
          "id": "wnd_title",
          "caption": "yes",
          "text": "#options/profilePilot"
        },
        "Button_close": {
          "id": "btn_back"
        }
      },
      "tdiv": {
        "id": "images_list",
        "flow": "h-flow",
        "width": "pw",
        "height": "ph",
        "behavior": "posNavigator",
        "canSelectNone": "yes",
        "navigatorShortcuts": "yes",
        "showSelect": "always",
        "on_click": "onImageChoose",
        "on_dbl_click": "onDblClick",
        "on_select": "onImageSelect"
      },
      "navBar": {
        "navMiddle": {
          "id": "paginator_place"
        },
        "navRight": {
          "Button_text": [
            {
              "id": "btn_select",
              "btnName": "A",
              "_on_click": "onAction",
              "visualStyle": "secondary",
              "buttonWink": {},
              "ButtonImg": {}
            },
            {
              "id": "btn_fav",
              "text": "#preloaderSettings/trackProgress",
              "btnName": "A",
              "on_click": "onToggleFav",
              "visualStyle": "secondary",
              "buttonWink": {},
              "ButtonImg": {}
            },
            {
              "id": "btn_buy",
              "text": "#mainmenu/btnOrder",
              "btnName": "A",
              "on_click": "onBuy",
              "visualStyle": "purchase",
              "buttonWink": {},
              "buttonGlance": {},
              "ButtonImg": {}
            }
          ]
        }
      }
    }
  }
}