{
  "root": {
    "type": "big",
    "blur": {},
    "blur_foreground": {},
    "frame": {
      "width": "1@researchNotificationWidth",
      "max-height": "1@maxWindowHeight",
      "class": "wndNav",
      "isCenteredUnderLogo": "yes",
      "frame_header": {
        "activeText": {
          "text": "#mainmenu/newResearch/title",
          "caption": "yes"
        },
        "Button_close": {}
      },
      "img": {
        "width": "pw",
        "height": "200.0/600pw",
        "background-image": "#ui/images/engineer",
        "pos": "50%pw-50%w,0",
        "position": "relative",
        "tdiv": {
          "size": "pw,ph",
          "position": "absolute",
          "background-color": "@shadeBackgroundColor4"
        },
        "rankUpList": {
          "id": "rankup_aircraft_table",
          "overflow-y": "auto",
          "scrollbarShortcuts": "yes",
          "position": "absolute",
          "pos": "50%pw-50%w, 50%ph-50%h",
          "holdTooltipChildren": "yes"
        }
      },
      "navBar": {
        "navLeft": {},
        "navRight": {
          "Button_text": [
            {
              "id": "btn_buy",
              "text": "#mainmenu/btnOrder",
              "btnName": "A",
              "_on_click": "purchaseUnit",
              "visualStyle": "purchase",
              "buttonWink": {},
              "buttonGlance": {},
              "ButtonImg": {}
            },
            {
              "id": "btn_trainCrew",
              "text": "#mainmenu/btnTrainCrew",
              "btnName": "A",
              "_on_click": "trainCrew",
              "visualStyle": "secondary",
              "buttonWink": {},
              "ButtonImg": {}
            },
            {
              "id": "btn_exit",
              "text": "#mainmenu/finish",
              "btnName": "B",
              "_on_click": "goBack",
              "ButtonImg": {}
            }
          ]
        }
      }
    }
  }
}