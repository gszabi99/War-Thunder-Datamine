{
  "root": {
    "blur": {},
    "blur_foreground": {},
    "frame": {
      "pos": "0.5pw-0.5w, 0.5ph-0.5h",
      "position": "absolute",
      "width": "80%sh",
      "max-width": "1080@sf/@pf+2@framePadding",
      "max-height": "@rh",
      "class": "wndNav",
      "frame_header": {
        "activeText": {
          "id": "promo_name",
          "width": "p.p.w-2@buttonHeight",
          "caption": "yes",
          "pare-text": "yes"
        },
        "Button_close": {
          "id": "btn_close"
        }
      },
      "img": {
        "id": "promo_image",
        "width": "pw",
        "height": "0.75w",
        "max-width": "1080@sf/@pf",
        "max-height": "810@sf/@pf",
        "position": "relative",
        "pos": "50%pw-50%w, 0",
        "background-image": "#ui/images/login_reward?P1"
      },
      "tdiv": {
        "width": "pw",
        "max-height": "fh",
        "position": "relative",
        "pos": "0, 1@framePadding",
        "overflow-y": "auto",
        "textarea": {
          "id": "promo_desc",
          "position": "relative",
          "pos": "50%pw-50%w, 0",
          "max-width": "pw",
          "removeParagraphIndent": "yes",
          "text": "",
          "font-bold": "@fontMedium",
          "on_link_click": "proccessLinkFromText"
        }
      },
      "navBar": {
        "navMiddle": {
          "paginatorPlace": {
            "id": "paginator_place",
            "smallPaginatorWidth": "yes"
          }
        },
        "navRight": {
          "Button_text": {
            "id": "btn_ok",
            "text": "#mainmenu/acceptObjective",
            "btnName": "X",
            "on_click": "onAcceptClick",
            "visualStyle": "secondary",
            "buttonWink": {},
            "ButtonImg": {}
          }
        }
      }
    }
  }
}