                    "id": "items_count",
                    "smallFont": "yes",
                    "valign": "center",
                    "position": "relative"
                  },
                  "text": {
                    "id": "max_items_count",
                    "tinyFont": "yes",
                    "valign": "center",
                    "position": "relative"
                  }
