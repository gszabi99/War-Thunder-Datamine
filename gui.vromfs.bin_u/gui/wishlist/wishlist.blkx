              "showButtonImageOnConsole": "no",
              "visualStyle": "purchase",
              "btnName": "X",
              "skip-navigation": "yes",
              "display": "hide",
              "enable": "no",
              "buttonWink": {},
              "buttonGlance": {},
              "ButtonImg": {},
              "textarea": {
                "id": "btnBuy_text",
                "class": "buttonText"
              },
              "discount": {
                "id": "buy_discount",
                "text": "",
                "pos": "pw-45%w, -80%h",
                "position": "absolute",
                "rotation": "-30"
              }
            },
            {
              "id": "btnMarketplace",
              "text": "#msgbox/btn_find_on_marketplace",
