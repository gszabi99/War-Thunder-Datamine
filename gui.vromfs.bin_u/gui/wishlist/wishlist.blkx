              },
              "btnText": {
                "id": "gc_balance",
                "min-width": "0.05@sf",
                "pos": "1@blockInterval, 50%ph-50%h",
                "position": "relative",
                "text-align": "left"
              },
              "BonusCorner": [
                {
                  "bonusType": "personal"
