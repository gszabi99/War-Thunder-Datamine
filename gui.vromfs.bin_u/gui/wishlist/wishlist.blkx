            },
            {
              "id": "btnConditions",
              "text": "#sm_conditions",
              "btnName": "X",
              "on_click": "onShowConditions",
              "showButtonImageOnConsole": "no",
              "visualStyle": "secondary",
              "skip-navigation": "yes",
              "display": "hide",
              "enable": "no",
              "buttonWink": {},
              "buttonGlance": {},
              "ButtonImg": {}
            }
          ]
