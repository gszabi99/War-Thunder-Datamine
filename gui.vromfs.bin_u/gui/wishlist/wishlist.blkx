              "on_click": "onUseCoupon",
              "showButtonImageOnConsole": "no",
              "visualStyle": "secondary",
              "skip-navigation": "yes",
              "class": "image",
              "display": "hide",
              "enable": "no",
              "buttonWink": {},
              "img": {
                "background-image": "#ui/gameuiskin#gc.svg"
              },
              "ButtonImg": {}
