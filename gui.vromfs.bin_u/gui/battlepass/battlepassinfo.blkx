[
  {
    "flow": "vertical"
  },
  {
    "padding": "2@blockInterval"
  },
  {
    "tdiv": [
      {
        "id": "battle_pass_info"
      },
      {
        "size": "pw, fh"
      },
      {
        "flow": "vertical"
      },
      {
        "overflow-y": "auto"
      },
      {
        "tdiv": {
          "id": "season_lvl",
          "behaviour": "bhvUpdateByWatched",
          "size": "pw, 1@battlePassRankInfoHeight",
          "textareaNoTab": {
            "top": "1@battlePassBigFlagTextAbsPos",
            "position": "relative",
            "text": "#mainmenu/rank",
            "bigBoldFont": "yes",
            "margin": "3@blockInterval, 0"
          },
          "battlePassFlag": {
            "top": "0.5ph-0.5h",
            "position": "relative",
            "margin-right": "3@blockInterval",
            "flagSize": "big",
            "textareaNoTab": {
              "id": "flag_text",
              "text": ""
            }
          },
          "tdiv": {
            "id": "level_exp",
            "behaviour": "bhvUpdateByWatched",
            "size": "fw, ph",
            "expProgress": {
              "id": "progress_bar",
              "width": "pw",
              "position": "relative",
              "pos": "0, 0.5ph-h",
              "value": "0",
              "big": "yes",
              "type": "battlePassRank",
              "activeText": {
                "id": "progress_text",
                "position": "absolute",
                "pos": "0.5pw-0.5w, 0.5ph-0.5h",
                "tinyFont": "yes"
              }
            }
          }
        }
      },
      {
        "battlePassInfoWatch": {
          "id": "today_login_exp",
          "textareaNoTab": {
            "id": "text"
          }
        }
      },
      {
        "battlePassInfoWatch": {
          "id": "login_streak",
          "textareaNoTab": {
            "id": "text"
          }
        }
      },
      {
        "battlePassInfoWatch": {
          "id": "tomorow_login_exp",
          "textareaNoTab": {
            "id": "text"
          }
        }
      },
      {
        "battlePassInfoWatch": {
          "id": "season_tasks_progress",
          "textareaNoTab": {
            "text": "#battlePass/info/season_tasks_progress"
          },
          "expProgress": {
            "id": "progress_bar",
            "width": "fw",
            "position": "relative",
            "pos": "0, 0.5ph-0.5h",
            "margin-left": "1@blockInterval",
            "value": "0",
            "big": "yes",
            "type": "battlePassTask",
            "activeText": {
              "id": "progress_text",
              "position": "absolute",
              "pos": "0.5pw-0.5w, 0.5ph-0.5h",
              "tinyFont": "yes",
              "text": ""
            }
          }
        }
      },
      {
        "tdiv": {
          "id": "task_timer_nest",
          "width": "pw + 4@blockInterval",
          "pos": "0.5pw-0.5w, 4@blockInterval",
          "position": "relative",
          "padding": "2@blockInterval, 2@dp",
          "background-color": "@separatorBlockColor",
          "textareaNoTab": {
            "id": "task_timer_text",
            "behavior": "Timer",
            "width": "pw-2@blockInterval",
            "pos": "pw-w, 0.5ph-0.5h",
            "position": "relative",
            "text": "",
            "smallFont": "yes"
          }
        }
      },
      {
        "battlePassInfoWatch": {
          "id": "easy_daily_task_progress",
          "css-hier-invalidate": "yes",
          "img": {
            "id": "task_img",
            "background-image": ""
          },
          "textareaNoTab": {
            "id": "text"
          },
          "icon": []
        }
      },
      {
        "battlePassInfoWatch": {
          "id": "medium_daily_task_progress",
          "css-hier-invalidate": "yes",
          "img": {
            "id": "task_img"
          },
          "textareaNoTab": {
            "id": "text"
          },
          "icon": []
        }
      },
      {
        "battlePassInfoWatch": {
          "id": "left_special_tasks_bought_count",
          "img": {
            "id": "task_img"
          },
          "textareaNoTab": {
            "id": "text"
          }
        }
      }
    ]
  },
  {
    "tdiv": [
      {
        "pos": "pw-w, 0"
      },
      {
        "position": "relative"
      },
      {
        "padding-top": "1@blockInterval"
      },
      {
        "Button_text": {
          "id": "btn_warbondsShop",
          "display": "hide",
          "enable": "no",
          "position": "relative",
          "noMargin": "yes",
          "visualStyle": "secondary",
          "btnName": "LT",
          "on_click": "onWarbondsShop",
          "buttonWink": [],
          "buttonGlance": [],
          "ButtonImg": [],
          "img": {
            "size": "1@cIco, 1@cIco",
            "background-svg-size": "1@cIco, 1@cIco",
            "background-image": "#ui/gameuiskin#wb.svg"
          },
          "text": {
            "margin-left": "1@blockInterval",
            "text": "#mainmenu/btnOnlineShop"
          }
        }
      },
      {
        "Button_text": {
          "id": "btn_battleTask",
          "pos": "1@blockInterval, 0",
          "position": "relative",
          "text": "#userlog/page/battletasks",
          "noMargin": "yes",
          "visualStyle": "secondary",
          "btnName": "L3",
          "on_click": "onBattleTask",
          "buttonWink": [],
          "buttonGlance": [],
          "ButtonImg": []
        }
      }
    ]
  }
]