{
  "root": {
    "blur": {},
    "blur_foreground": {},
    "gamercardShadePlace": {
      "gamercardShade": {}
    },
    "frame": {
      "id": "recycle_frame",
      "min-width": "1@trophyWndWidth",
      "max-width": "1@rh",
      "class": "wndNav",
      "type": "big",
      "isCenteredUnderLogo": "yes",
      "css-hier-invalidate": "yes",
      "flow": "h-flow",
      "frame_header": {
        "textareaNoTab": {
          "id": "reward_title",
          "caption": "yes",
          "overlayTextColor": "active",
          "text": "#items/recycledComplete"
        },
        "Button_close": {
          "id": "btn_back"
        }
      },
      "tdiv": {
        "position": "relative",
        "flow": "vertical",
        "flow-align": "center",
        "left": "(pw-w)/2",
        "width": "pw",
        "css-hier-invalidate": "yes",
        "div": {
          "id": "scroll_container",
          "width": "pw",
          "max-height": "0.7@rh",
          "overflow-y": "auto",
          "div": {
            "id": "reward_image_place",
            "width": "pw",
            "css-hier-invalidate": "yes",
            "position": "relative",
            "flow": "h-flow",
            "flow-align": "center",
            "display": "show",
            "total-input-transparent": "yes"
          }
        },
        "tdiv": {
          "size": "pw, 1@sf/@pf",
          "background-color": "#2d343c"
        },
        "textareaNoTab": {
          "id": "extended_desc",
          "position": "relative",
          "left": "(pw-w)/2",
          "max-width": "pw",
          "padding": "25@sf/@pf, 12@sf/@pf, 25@sf/@pf, 10@sf/@pf",
          "hideEmptyText": "yes",
          "text": "#items/recycledNewItems",
          "text-align": "center"
        }
      },
      "navBar": {
        "navLeft": {},
        "navRight": {
          "Button_text": {
            "id": "btn_ok",
            "text": "#mainmenu/btnOk",
            "btnName": "A",
            "_on_click": "goBack",
            "ButtonImg": {}
          }
        }
      }
    }
  }
}