{
  "chapterListPlace": [
    {
      "id": "chapter_place"
    },
    {
      "height": "ph"
    },
    {
      "flow": "vertical"
    },
    {
      "tdiv": {
        "id": "div_before_chapters_list",
        "width": "pw",
        "display": "hide"
      }
    },
    {
      "tdiv": {
        "id": "chapters_list_place",
        "size": "pw, fh",
        "flow": "vertical",
        "popupFilter": {
          "margin-left": "1@buttonMargin"
        },
        "tdiv": {
          "id": "listbox_filter_holder",
          "width": "pw",
          "pos": "1@blockInterval, 0",
          "position": "relative"
        },
        "listbox": {
          "id": "items_list",
          "size": "pw, fh",
          "flow": "vertical",
          "move-only-hover": "yes",
          "navigatorShortcuts": "yes",
          "on_select": "onItemSelect",
          "_on_dbl_click": "onItemDblClick"
        },
        "textAreaCentered": {
          "id": "items_list_msg",
          "width": "80%pw",
          "pos": "50%pw-50%w, 40%ph-40%h",
          "position": "absolute",
          "hideEmptyText": "yes",
          "text": ""
        },
        "animated_wait_icon": {
          "id": "items_list_wait_icon",
          "pos": "50%pw-50%w, 40%ph-40%h",
          "position": "absolute",
          "background-rotation": "0",
          "display": "hide"
        }
      }
    }
  ],
  "chapterSeparator": {
    "margin": "1@blockInterval, 0"
  },
  "tdiv": [
    {
      "id": "item_desc"
    },
    {
      "flow": "vertical"
    },
    {
      "size": "fw, ph"
    },
    {
      "overflow-y": "auto"
    },
    {
      "total-input-transparent": "yes"
    },
    {
      "text": {
        "left": "(pw-w)/2",
        "id": "item_name",
        "position": "relative",
        "style": "font:@fontMedium;color:@titleTextColor;",
        "textHide": "yes",
        "text": ""
      }
    },
    {
      "tdiv": {
        "id": "mission_desc",
        "size": "pw,ph",
        "position": "absolute",
        "flow": "vertical"
      }
    },
    {
      "tdiv": [
        {
          "id": "chapter_include_block"
        },
        {
          "size": "pw, fh"
        },
        {
          "flow": "vertical"
        },
        {
          "display": "hide"
        },
        {
          "img": {
            "id": "item_desc_header_img",
            "width": "pw",
            "height": "w*128/1024",
            "background-repeat": "expand",
            "background-image": ""
          }
        },
        {
          "tdiv": {
            "id": "div_before_text",
            "width": "pw",
            "flow": "h-flow",
            "flow-align": "center"
          }
        },
        {
          "tdiv": {
            "height": "fh",
            "width": "pw",
            "overflow-y": "auto",
            "flow": "vertical",
            "scrollbarShortcuts": "yes",
            "total-input-transparent": "yes",
            "textarea": {
              "id": "item_desc_text",
              "width": "pw",
              "chapterTextAreaStyle": "yes",
              "hideEmptyText": "yes",
              "text": "",
              "font-bold": "@fontMedium"
            },
            "tdiv": {
              "id": "session_results",
              "width": "pw",
              "padding-top": "0.01@scrn_tgt",
              "flow": "vertical",
              "display": "hide",
              "textarea": {
                "chapterTextAreaStyle": "yes",
                "font-bold": "@fontMedium",
                "text": "#mainmenu/sessionResults"
              },
              "tdiv": {
                "id": "results_table_place",
                "width": "pw",
                "flow": "vertical"
              }
            }
          }
        }
      ]
    },
    {
      "tdiv": {
        "id": "after_desc_block",
        "width": "pw"
      }
    },
    {
      "tdiv": {
        "id": "item_misc",
        "flow": "vertical"
      }
    }
  ]
}