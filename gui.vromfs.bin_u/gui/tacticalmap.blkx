{
  "img": {
    "position": "absolute",
    "size": "3sw - 3sw*@isInVr, sh/3+1",
    "pos": "-sw,sh-h-1@bh",
    "background-image": "#ui/gameuiskin#optionlist_background_down"
  },
  "root": {
    "flow": "vertical",
    "background-color": "@shadeBackgroundColor",
    "type": "big"
  },
  "tdiv": [
    {
      "id": "hintBlock",
      "pos": "0.5pw - 0.5w, ph - h - @hudActionBarItemHeight",
      "position": "absolute",
      "tdiv": {
        "id": "hud_hints_nest",
        "width": "2/3@rwHud",
        "flow": "vertical",
        "css-hier-invalidate": "yes",
        "behavior": "Timer",
        "tdiv": [
          {
            "id": "ship_obstacle_rf",
            "size": "pw, 0.04@shHud"
          },
          {
            "width": "pw",
            "position": "relative",
            "pos": "pw/2 - w/2, 0",
            "flow": "vertical",
            "tdiv": [
              {
                "id": "common_priority_hints_holder",
                "position": "relative",
                "tdiv": {
                  "id": "common_priority_hints",
                  "width": "pw",
                  "position": "relative",
                  "pos": "0, ph-h",
                  "flow": "vertical"
                }
              },
              {
                "width": "pw",
                "tdiv": {
                  "id": "mission_hints",
                  "width": "pw",
                  "position": "relative",
                  "pos": "0, ph-h",
                  "flow": "vertical"
                }
              },
              {
                "id": "mission_action_hints_holder",
                "position": "relative",
                "tdiv": {
                  "id": "mission_action_hints",
                  "width": "pw",
                  "position": "relative",
                  "pos": "0, ph-h",
                  "flow": "vertical"
                }
              }
            ]
          },
          {
            "position": "relative",
            "left": "pw/2 - w/2",
            "div": [
              {
                "id": "iconed_hints"
              },
              {
                "id": "display_timers"
              }
            ]
          },
          {
            "id": "actionbar_hints_nest",
            "width": "1@shHud",
            "left": "0.5pw-0.5w",
            "position": "relative",
            "tdiv": {
              "width": "pw",
              "position": "relative",
              "pos": "0, ph-h",
              "flow": "vertical",
              "tdiv": {
                "id": "actionbar_hints",
                "width": "pw",
                "flow": "vertical"
              }
            }
          },
          {
            "id": "tutorial_hints",
            "width": "pw",
            "position": "root",
            "pos": "50%sw-50%w, 1@bhHud + 0.67@shHud",
            "flow": "vertical"
          },
          {
            "id": "minor_priority_hints",
            "width": "pw",
            "position": "root",
            "pos": "50%sw-50%w, @shHud - h",
            "flow": "vertical"
          }
        ],
        "hudMessages": [
          {
            "id": "hud_message_zone_capture_notification",
            "width": "pw",
            "message_style": "zone_capture_notification",
            "input-transparent": "yes"
          },
          {
            "id": "hud_message_player_damage_notification",
            "message_style": "player_damage_notification",
            "left": "0.2@shHud",
            "position": "relative",
            "flow": "vertical",
            "width": "pw - 0.2@shHud"
          },
          {
            "id": "hud_message_tank_notification",
            "message_style": "player_damage_notification",
            "left": "0.2@shHud",
            "position": "relative",
            "flow": "vertical",
            "width": "pw - 0.2@shHud"
          },
          {
            "id": "hud_message_ship_notification",
            "message_style": "player_damage_notification",
            "left": "0.2@shHud",
            "position": "relative",
            "flow": "vertical",
            "width": "pw - 0.2@shHud"
          }
        ],
        "textAreaCentered": {
          "id": "low-quality-model-warning",
          "width": "pw",
          "style": "color:@warningTextColor; font:@fontHudNormal;",
          "text": "#low_quality_model_in_hangar",
          "display": "hide"
        }
      }
    },
    {
      "position": "absolute",
      "pos": "@titlePos,1@topBarHeight",
      "flow": "vertical",
      "smallFont": "yes",
      "textareaNoTab": [
        {
          "id": "menu-title",
          "overlayTextColor": "active",
          "mediumFont": "yes",
          "textShade": "yes",
          "text": ""
        },
        {
          "id": "conditions_text",
          "overlayTextColor": "premiumNotEarned",
          "textShade": "yes",
          "text": ""
        }
      ]
    },
    {
      "id": "update_timer",
      "behaviour": "Timer",
      "timer_handler_func": "update"
    }
  ],
  "optionlist_container": {
    "id": "flight_menu_bgd",
    "type": "big",
    "style": "padding:10@sf/@pf;",
    "tdiv": [
      {
        "id": "objectives_panel",
        "size": "50%pw, ph",
        "max-width": "pw-ph",
        "overflow": "hidden",
        "display": "hide",
        "optionlist_line_left": [
          {
            "size": "pw,5*@scrn_tgt/100.0",
            "pos": "0,-0.3*@scrn_tgt/100.0",
            "position": "relative",
            "activeText": {
              "text": "#map/primary_objectives",
              "pare-text": "yes",
              "left": "-1*@scrn_tgt/100.0",
              "width": "pw",
              "style": "text-align:right;"
            }
          },
          {
            "size": "pw,5*@scrn_tgt/100.0",
            "activeText": {
              "text": "#map/secondary_objectives",
              "pare-text": "yes",
              "left": "-1*@scrn_tgt/100.0",
              "width": "pw",
              "style": "text-align:right;"
            }
          }
        ],
        "_newline": [
          {},
          {},
          {}
        ],
        "div": [
          {
            "width": "pw",
            "height": "53%ph",
            "overflow": "hidden",
            "tdiv": {
              "width": "pw",
              "min-height": "ph",
              "pos": "0,ph-h",
              "position": "relative",
              "padding-bottom": "0.8*@scrn_tgt/100.0",
              "flow": "vertical",
              "id": "primary_tasks_list"
            }
          },
          {
            "width": "pw",
            "height": "fh",
            "overflow": "hidden",
            "tdiv": {
              "id": "secondary_tasks_list",
              "width": "pw",
              "min-height": "ph",
              "pos": "0,ph-h",
              "position": "relative",
              "padding-bottom": "0.8*@scrn_tgt/100.0",
              "flow": "vertical"
            }
          }
        ]
      },
      {
        "id": "pilots_panel",
        "size": "50%pw, ph",
        "max-width": "pw-ph",
        "overflow": "hidden",
        "optionlist_line_left": {
          "size": "pw,5*@scrn_tgt/100.0",
          "activeText": {
            "text": "#map/pilot_list",
            "pare-text": "yes",
            "tdalign": "right"
          }
        },
        "_newline": {},
        "tdiv": {
          "size": "pw, 16@rows16height + 4",
          "overflow-y": "auto",
          "margin-top": "1*@scrn_tgt/100.0",
          "scroll-align": "left",
          "padding-left": "10+0.5*@scrn_tgt/100.0",
          "table": {
            "width": "pw",
            "id": "pilots_list",
            "baseRow": "yes",
            "selType": "left",
            "text-valign": "center",
            "behavior": "PosNavigator",
            "selfFocusBorder": "yes",
            "total-input-transparent": "yes",
            "navigatorShortcuts": "yes",
            "css-hier-invalidate": "all",
            "on_dbl_click": "onPilotsDblClick",
            "on_click": "onPilotsSelect",
            "on_activate": "onPilotsSelect"
          }
        }
      },
      {
        "size": "ph, ph",
        "min-width": "50%pw",
        "div": {
          "behaviour": "button",
          "id": "POI_resetter",
          "size": "sw,sh",
          "position": "root",
          "on_click": "onRespawnScreenClick"
        },
        "tacticalMap": {
          "size": "ph,ph",
          "id": "tactical-map",
          "tdiv": {
            "pos": "pw+1@blockInterval, ph-h",
            "position": "absolute",
            "flow": "vertical",
            "background-color": "@editboxColor",
            "tdiv": [
              {
                "id": "hint_attention_to_map",
                "margin": "1@blockInterval",
                "tooltip": "#voice_message_attention_to_point_2",
                "display": "hide",
                "div": [
                  {
                    "size": "1@buttonHeight, 1@buttonHeight",
                    "input-transparent": "yes",
                    "img": {
                      "size": "1@cIco, 1@cIco",
                      "pos": "0.5pw-0.5w, 0.5ph-0.5h",
                      "position": "relative",
                      "background-svg-size": "@cIco, @cIco",
                      "background-image": "#ui/gameuiskin#new_icon.svg"
                    }
                  },
                  {
                    "size": "1@buttonHeight, 1@buttonHeight",
                    "margin-left": "1@blockInterval",
                    "input-transparent": "yes",
                    "img": {
                      "size": "0.73h, 1@cIco",
                      "pos": "0.5pw-0.5w, 0.5ph-0.5h",
                      "position": "relative",
                      "background-svg-size": "0.73h, @cIco",
                      "background-image": "#ui/gameuiskin#mouse_left.avif"
                    }
                  }
                ]
              },
              {
                "margin": "1@blockInterval",
                "tooltip": "#options/rmbToMoveMap",
                "Button_text": {
                  "class": "image",
                  "btnName": "RB",
                  "noMargin": "yes",
                  "tooltip": "#options/moveMap",
                  "_on_click": "onMoveMapActivate",
                  "ButtonImg": {},
                  "img": {
                    "background-image": "#ui/gameuiskin#dnd_icon.svg"
                  }
                },
                "tdiv": {
                  "id": "hint_btn_move_map",
                  "size": "1@buttonHeight, 1@buttonHeight",
                  "margin-left": "1@blockInterval",
                  "display": "hide",
                  "img": {
                    "size": "0.73h, 1@cIco",
                    "position": "relative",
                    "pos": "0.5pw-0.5w, 0.5ph-0.5h",
                    "background-svg-size": "0.73h, 1@cIco",
                    "background-image": "#ui/gameuiskin#mouse_right.avif"
                  }
                }
              },
              {
                "id": "btn_set_point_of_interest",
                "margin": "1@blockInterval",
                "display": "hide",
                "Button_text": {
                  "class": "image",
                  "btnName": "LT",
                  "noMargin": "yes",
                  "on_click": "onSetPointOfInterest",
                  "tooltip": "#options/setPointOfInterest",
                  "ButtonImg": {},
                  "img": {
                    "id": "hud_poi_img",
                    "background-image": "#ui/gameuiskin#map_interestpoint.svg"
                  }
                },
                "tdiv": {
                  "id": "hint_btn_set_point_of_interest",
                  "size": "1@buttonHeight, 1@buttonHeight",
                  "margin-left": "1@blockInterval",
                  "display": "hide",
                  "img": {
                    "size": "0.73h, 1@cIco",
                    "position": "relative",
                    "pos": "0.5pw-0.5w, 0.5ph-0.5h",
                    "tooltip": "#options/mmbToSetPoi",
                    "background-image": "#ui/gameuiskin#mouse_center.avif"
                  }
                }
              },
              {
                "id": "btn_set_hud_type",
                "margin": "1@blockInterval",
                "display": "hide",
                "Button_text": {
                  "class": "image",
                  "accessKey": "N | J:LB",
                  "noMargin": "yes",
                  "tooltip": "#options/forceHudType",
                  "on_click": "onForcedSetHudType",
                  "ButtonImg": {
                    "btnName": "LB"
                  },
                  "img": {
                    "id": "hud_type_img",
                    "background-image": "#ui/gameuiskin#objective_tank.svg"
                  }
                },
                "loadingHint": {
                  "id": "hint_btn_set_hud_type",
                  "size": "1@buttonHeight, 1@buttonHeight",
                  "margin-left": "1@blockInterval",
                  "display": "hide",
                  "keyboardButton": {
                    "position": "relative",
                    "pos": "0.5pw-0.5w, 0.5ph-0.5h",
                    "keyboardButtonText": {
                      "position": "relative",
                      "pos": "0.5pw - 0.5w, 0.5ph - 0.5h",
                      "text": "N"
                    }
                  }
                }
              }
            ]
          }
        }
      }
    ]
  },
  "navBarStatic": {
    "navLeft": {
      "activeText": {
        "id": "pilot_aircraft",
        "margin-left": "1@buttonMargin",
        "text": ""
      }
    }
  },
  "navBar": {
    "navRight": {
      "Button_text": [
        {
          "id": "btn_select",
          "text": "#mainmenu/btnSelect",
          "btnName": "A",
          "on_click": "onStart",
          "skip-navigation": "yes",
          "ButtonImg": {}
        },
        {
          "id": "btn_back",
          "text": "#mainmenu/btnBack",
          "btnName": "B",
          "_on_click": "goBack",
          "display": "hide",
          "skip-navigation": "yes",
          "ButtonImg": {}
        }
      ]
    }
  },
  "root_button": {
    "id": "screen_button_back",
    "display": "hide",
    "position": "root",
    "size": "sw,sh",
    "behavior": "button",
    "_on_click": "goBack"
  }
}