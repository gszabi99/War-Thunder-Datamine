{
  "img": {
    "position": "absolute",
    "size": "3sw - 3sw*@isInVr, sh/3+1",
    "pos": "-sw,sh-h-1@bh",
    "background-image": "#ui/gameuiskin#optionlist_background_down"
  },
  "root": {
    "flow": "vertical",
    "background-color": "@shadeBackgroundColor",
    "type": "big"
  },
  "titletext": {
    "id": "menu-title",
    "type": "big"
  },
  "optionlist_container": [
    {
      "id": "flight_menu_bgd"
    },
    {
      "type": "big"
    },
    {
      "style": "padding:10@sf/@pf;"
    },
    {
      "tdiv": [
        {
          "id": "objectives_panel"
        },
        {
          "size": "50%pw, ph"
        },
        {
          "max-width": "pw-ph"
        },
        {
          "overflow": "hidden"
        },
        {
          "display": "hide"
        },
        {
          "optionlist_line_left": {
            "size": "pw,5*@scrn_tgt/100.0",
            "pos": "0,-0.3*@scrn_tgt/100.0",
            "position": "relative",
            "activeText": {
              "text": "#map/primary_objectives",
              "pare-text": "yes",
              "left": "-1*@scrn_tgt/100.0",
              "width": "pw",
              "style": "text-align:right;"
            }
          }
        },
        {
          "optionlist_line_left": {
            "size": "pw,5*@scrn_tgt/100.0",
            "activeText": {
              "text": "#map/secondary_objectives",
              "pare-text": "yes",
              "left": "-1*@scrn_tgt/100.0",
              "width": "pw",
              "style": "text-align:right;"
            }
          }
        },
        {
          "_newline": []
        },
        {
          "_newline": []
        },
        {
          "_newline": []
        },
        {
          "div": {
            "width": "pw",
            "height": "53%ph",
            "overflow": "hidden",
            "tdiv": {
              "width": "pw",
              "min-height": "ph",
              "pos": "0,ph-h",
              "position": "relative",
              "padding-bottom": "0.8*@scrn_tgt/100.0",
              "flow": "vertical",
              "id": "primary_tasks_list"
            }
          }
        },
        {
          "div": {
            "width": "pw",
            "height": "fh",
            "overflow": "hidden",
            "tdiv": {
              "id": "secondary_tasks_list",
              "width": "pw",
              "min-height": "ph",
              "pos": "0,ph-h",
              "position": "relative",
              "padding-bottom": "0.8*@scrn_tgt/100.0",
              "flow": "vertical"
            }
          }
        }
      ]
    },
    {
      "tdiv": {
        "id": "pilots_panel",
        "size": "50%pw, ph",
        "max-width": "pw-ph",
        "overflow": "hidden",
        "optionlist_line_left": {
          "size": "pw,5*@scrn_tgt/100.0",
          "activeText": {
            "text": "#map/pilot_list",
            "pare-text": "yes",
            "tdalign": "right"
          }
        },
        "_newline": [],
        "tdiv": {
          "size": "pw, 16@rows16height + 4",
          "overflow-y": "auto",
          "margin-top": "1*@scrn_tgt/100.0",
          "scroll-align": "left",
          "padding-left": "10+0.5*@scrn_tgt/100.0",
          "table": {
            "width": "pw",
            "id": "pilots_list",
            "baseRow": "yes",
            "selType": "left",
            "text-valign": "center",
            "behavior": "PosNavigator",
            "selfFocusBorder": "yes",
            "total-input-transparent": "yes",
            "navigatorShortcuts": "yes",
            "css-hier-invalidate": "all",
            "on_dbl_click": "onStart",
            "on_click": "onPilotsSelect"
          }
        }
      }
    },
    {
      "tdiv": {
        "size": "ph, ph",
        "min-width": "50%pw",
        "tacticalMap": {
          "size": "ph,ph"
        }
      }
    }
  ],
  "navBarStatic": {
    "navLeft": {
      "activeText": {
        "id": "pilot_aircraft",
        "margin-left": "1@buttonMargin",
        "text": ""
      }
    }
  },
  "navBar": {
    "navRight": [
      {
        "Button_text": {
          "id": "btn_select",
          "text": "#mainmenu/btnSelect",
          "btnName": "A",
          "_on_click": "onSelect",
          "ButtonImg": []
        }
      },
      {
        "Button_text": {
          "id": "btn_back",
          "text": "#mainmenu/btnBack",
          "btnName": "B",
          "_on_click": "goBack",
          "display": "hide",
          "ButtonImg": []
        }
      }
    ]
  },
  "root_button": {
    "id": "screen_button_back",
    "display": "hide",
    "position": "root",
    "size": "sw,sh",
    "behavior": "button",
    "_on_click": "goBack"
  },
  "dummy": {
    "_on_click": "onSelect",
    "behaviour": "accesskey",
    "btnName": "B"
  },
  "tdiv": {
    "id": "update_timer",
    "behaviour": "Timer",
    "timer_handler_func": "update"
  }
}