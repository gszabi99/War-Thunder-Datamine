"@delete:dx12" {}
dx12{
  debug {
    loadPixCapturer:b=no
  }
  onlyPreallocatedHeaps:b=no
  preallocHeap { sizeMB:i=1024; }
  preallocHeap { sizeMB:i=32;   cpuCached:b=yes; }
  preallocHeap { sizeMB:i=4;    gpuExec:b=yes; }
}

"@override:video" {
  mode:t="windowed"
  vsync:b=yes
  adaptive_vsync:b=no
  vrMode:b=no
  instancing:t="auto"
  compatibilityMode:b=no
  antialiasing_mode:t="tsr"
  antialiasing_upscaling:t="native"
  driver:t="auto"
  disableFlipEx:b=no
  alpha_to_coverage:b=yes
  windowed:b=yes
  taa:i=0
}

"@override:graphics" {
  advancedShore:b=yes
  forceEnemiesLowQuality:b=yes
  enableSuspensionAnimation:b=yes
  clipmapScale:r=1
  cloudsQuality:i=1
  backgroundScale:r=1
  anisotropy:i=16
  lenseFlares:b=yes
  planarReflectionTexDiv:i=5
  waterReflectionTexDiv:i=2
  physicsQuality:i=4
  skyQuality:i=1
  texquality:t="high"
  dirtSubDiv:i=2
  landquality:i=2
  waterRefractionEnabledFor:i=2
  lastClipSize:i=8192
  panoramaResolution:i=2560
  waterQuality:t="high"
  giQuality:t="medium"
  dynamicHoles:b=yes
}

"@override:render" {
  waterReflection:b=yes
  wake:b=yes
  softFx:b=yes
  ssaoQuality:i=2
  haze:b=yes
  rendinstGlobalShadows:b=yes
  shadows:b=yes
  ssrQuality:i=0
}

"@override:gi" {
  dagiAlgorithm:r = 0.4
}
