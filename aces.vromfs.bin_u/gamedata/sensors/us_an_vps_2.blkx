          }
        },
        "detect": {
          "event": "targetInSight",
          "actions": {
            "detectTarget": {
              "ignoreOwnWeapon": true,
              "rangeMult": 1.0
            }
          }
        }
      }
    },
    "track": {
      "stateInit": "track",
      "transitions": {
        "detect": {
          "event": "update",
          "actions": {
            "detectTarget": {
              "ignoreOwnWeapon": true,
              "rangeMult": 1.0
            }
          }
        },
        "track": {
          "event": "targetDetected",
          "actions": {
            "updateActiveTargetOfInterest": {
              "dirFilter": {
                "rateLim": [
                  0.78,
                  0.78
                ],
                "limTimeOut": 1.0,
                "inertialTimeMax": 1.0
              },
              "distFilter": {
                "accelLim": [
                  150.0,
                  150.0
                ],
                "limTimeOut": 1.0,
                "inertialTimeMax": 1.0
              }
            },
            "setCenterAzimuth": {
              "source": "activeTargetOfInterest"
            },
            "setCenterElevation": {
              "source": "activeTargetOfInterest"
            },
            "setDistGatePos": {
              "source": "activeTargetOfInterest",
              "width": 0.0
            }
          }
        },
        "extrapolate": {
          "event": "targetNotDetected",
          "actions": {
            "extrapolateTargetsOfInterest": {},
            "clearTargetsOfInterest": {
              "timeOut": 2.0,
              "dirRateLim": 0.78
            },
            "setCenterAzimuth": {
              "source": "activeTargetOfInterest"
            },
            "setCenterElevation": {
              "source": "activeTargetOfInterest"
            },
            "setDistGatePos": {
              "source": "activeTargetOfInterest",
              "width": 0.0
            }
          }
        }
      }
