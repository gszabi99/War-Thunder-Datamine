          "event": "scanFinished",
          "stateTo": "standby",
          "actions": {
            "setStandbyMode": {},
            "setFsmActive": {
              "fsm": "lock",
              "active": false
            }
          }
        },
        "finishedTrack": {
          "stateFrom": "track",
          "event": "targetsOfInterestCleanup",
          "command": [
            "selectTarget",
            "designateTarget"
          ],
          "stateTo": "standby",
          "actions": {
            "setStandbyMode": {},
            "setFsmActive": {
              "fsm": "track",
              "active": false
            }
          }
        }
