{
  "type": "radar",
  "canMoveCamera": true,
  "name": "Auto tracker",
  "showOnHud": false,
  "designationSourceType": 0,
  "transivers": {
    "track": {
      "power": 60.0,
      "active": false,
      "band": -1,
      "rcs": 3000.0,
      "range": 10000.0,
      "rangeMax": 15000.0,
      "visibilityType": "optic",
      "antenna": {
        "angleHalfSens": 0.05,
        "sideLobesSensitivity": -120.0
      }
    }
  },
  "scanPatterns": {
    "lock": {
      "type": "cone",
      "period": 0.2,
      "width": 0.01,
      "azimuthLimits": [
        -180.0,
        180.0
      ],
      "elevationLimits": [
        -180.0,
        180.0
      ],
      "yawStabLimit": 180.0,
      "pitchStabLimit": 180.0,
      "rollStabLimit": [
        180.0,
        180.0
      ],
      "pitchStabMandatory": true
    },
    "losLock": {
      "type": "cone",
      "period": 0.2,
      "width": 0.01,
      "azimuthLimits": [
        -180.0,
        180.0
      ],
      "elevationLimits": [
        -180.0,
        180.0
      ],
      "yawStabLimit": 180.0,
      "pitchStabLimit": 180.0,
      "rollStabLimit": [
        180.0,
        180.0
      ],
      "pitchStabMandatory": true
    },
    "track": {
      "type": "no",
      "azimuthLimits": [
        -180.0,
        180.0
      ],
      "elevationLimits": [
        -180.0,
        180.0
      ],
      "yawStabLimit": 180.0,
      "pitchStabLimit": 180.0,
      "rollStabLimit": 180.0,
      "pitchStabMandatory": true
    }
  },
  "signals": {
    "track": {
      "anglesFinder": true,
      "rangeFinder": true,
      "groundClutter": false,
      "aircraftAsTarget": true,
      "groundVehiclesAsTarget": true,
      "angularAccuracy": 0.0,
      "distanceAccuracy": 0.0,
      "detectWeapon": false,
      "distance": {
        "presents": true,
        "minValue": 100.0,
        "width": 200.0,
        "maxValue": 15000.0
      },
      "angles": {
        "presents": true,
        "resolution": 0.0001
      }
    }
  },
  "fsms": {
    "main": {
      "stateInit": "init",
      "actionsTemplates": {
        "init": {
          "setEnabled": {
            "value": false
          },
          "setTargetDesignationRange": [
            {},
            {
              "azimuthRange": [
                -180.0,
                180.0
              ],
              "elevationRange": [
                -180.0,
                180.0
              ],
              "distanceRange": [
                0.0,
                15000.0
              ]
            }
          ],
          "setTransiver": {
            "transiver": "track"
          },
          "setSignal": {
            "signal": "track"
          }
        },
        "setStandbyMode": {
          "setModeName": {
            "name": "standby"
          },
          "setEnabled": {
            "value": false
          },
          "setScanPattern": {
            "scanPattern": "lock"
          },
          "setCenterAzimuth": {
            "source": "constant",
            "value": 0.0
          },
          "clearTargetsOfInterest": {},
          "setFsmActive": [
            {
              "fsm": "lock",
              "active": false
            },
            {
              "fsm": "track",
              "active": false
            }
          ]
        },
        "setLockModeCommon1": {
          "setModeName": {
            "name": "acquisition"
          },
          "setWeaponDetectionAllowance": {
            "source": "const",
            "value": true
          },
          "setDesignatedTarget": {
            "set": true
          },
          "setEnabled": {
            "value": true
          }
        },
        "setLockModeCommon2": {
          "resetScanPhase": {},
          "setCenterAzimuth": {
            "source": "targetDesignation"
          },
          "clearTargetsOfInterest": {},
          "setFsmActive": {
            "fsm": "lock",
            "active": true
          }
        },
        "setLockModeCue": {
          "setLockModeCommon1": {},
          "setScanPattern": {
            "scanPattern": "lock"
          },
          "setLockModeCommon2": {},
          "setCenterElevation": {
            "source": "targetDesignation"
          },
          "setDistGatePos": {
            "source": "targetDesignation",
            "width": 50.0
          },
          "setAngularSize": {
            "source": "const",
            "value": 0.05
          }
        },
        "setLockModeTarget": {
          "setLockModeCommon1": {},
          "setScanPattern": {
            "scanPattern": "lock"
          },
          "setLockModeCommon2": {},
          "setCenterElevation": {
            "source": "targetDesignation"
          },
          "setDistGatePos": {
            "source": "targetDesignation",
            "width": 50.0
          },
          "setAngularSize": {
            "source": "const",
            "value": 0.05
          }
        },
        "setLosVisualLockModeCommon": {
          "setEnabled": [
            {
              "value": true
            },
            {
              "value": true
            }
          ],
          "setScanPattern": {
            "scanPattern": "losLock"
          },
          "resetScanPhase": {},
          "clearTargetsOfInterest": {},
          "setCenterAzimuth": {
            "source": "targetDesignation"
          },
          "setCenterElevation": {
            "source": "targetDesignation"
          },
          "setFsmActive": {
            "fsm": "lock",
            "active": true
          },
          "setModeName": {
            "name": "acquisition"
          }
        },
        "setLosLockMode": {
          "setLosVisualLockModeCommon": {},
          "setWeaponDetectionAllowance": {
            "source": "targetDesignation"
          },
          "setDesignatedTarget": {
            "set": false
          },
          "setDistGatePos": {
            "source": "constRange",
            "pos": 7500.0,
            "width": 15000.0
          },
          "setAngularSize": {
            "source": "const",
            "value": 0.05
          }
        },
        "setVisualLockMode": {
          "setLosVisualLockModeCommon": {},
          "setWeaponDetectionAllowance": {
            "source": "const",
            "value": true
          },
          "setDesignatedTarget": {
            "set": true
          },
          "setDistGatePos": {
            "source": "targetDesignation",
            "width": 50.0
          },
          "setAngularSize": {
            "source": "const",
            "value": 0.05
          }
        },
        "setTrackMode": {
          "setModeName": {
            "name": "track"
          },
          "setEnabled": {
            "value": true
          },
          "setWeaponDetectionAllowance": {
            "source": "const",
            "value": true
          },
          "setDesignatedTarget": {
            "set": false
          },
          "setScanPattern": {
            "scanPattern": "track"
          },
          "clearTargetsOfInterest": {},
          "addTargetOfInterest": {},
          "setLastTargetOfInterestActive": {},
          "updateActiveTargetOfInterest": {},
          "setCenterAzimuth": {
            "source": "activeTargetOfInterest"
          },
          "setCenterElevation": {
            "source": "activeTargetOfInterest"
          },
          "setDistGatePos": {
            "source": "activeTargetOfInterest",
            "width": 0.0
          },
          "setFsmActive": {
            "fsm": "track",
            "active": true
          }
        }
      },
      "transitions": {
        "init": {
          "stateFrom": "init",
          "event": "init",
          "stateTo": "standby",
          "actions": {
            "init": {},
            "setStandbyMode": {}
          }
        },
        "checkLockTypeCue": {
          "stateFrom": "standby",
          "command": "designateTarget",
          "stateTo": "checkLockTypeCue",
          "actions": {
            "checkDesignationTypeEquals": {
              "type": "cue"
            }
          }
        },
        "lockCue": {
          "stateFrom": "checkLockTypeCue",
          "event": "designationTypeEquals",
          "stateTo": "lockCue",
          "actions": {
            "setLockModeCue": {}
          }
        },
        "checkLockTypeTarget": {
          "stateFrom": "checkLockTypeCue",
          "event": "designationTypeNotEquals",
          "stateTo": "checkLockTypeTarget",
          "actions": {
            "checkDesignationTypeEquals": {
              "type": "target"
            }
          }
        },
        "lockTarget": {
          "stateFrom": "checkLockTypeTarget",
          "event": "designationTypeEquals",
          "stateTo": "lockTarget",
          "actions": {
            "setLockModeTarget": {}
          }
        },
        "checkLockTypeLos": [
          {
            "stateFrom": "checkLockTypeTarget",
            "event": "designationTypeNotEquals",
            "stateTo": "checkLockTypeLos",
            "actions": {
              "checkDesignationTypeEquals": {
                "type": "los"
              }
            }
          },
          {
            "stateFrom": "checkLockTypeLos",
            "event": "designationTypeNotEquals",
            "stateTo": "checkLockTypeVisual",
            "actions": {
              "checkDesignationTypeEquals": {
                "type": "visual"
              }
            }
          }
        ],
        "lockLos": {
          "stateFrom": "checkLockTypeLos",
          "event": "designationTypeEquals",
          "stateTo": "losLock",
          "actions": {
            "setLosLockMode": {}
          }
        },
        "lockVisual": {
          "stateFrom": "checkLockTypeVisual",
          "event": "designationTypeEquals",
          "stateTo": "visualLock",
          "actions": {
            "setVisualLockMode": {}
          }
        },
        "finishTypeCheck": {
          "stateFrom": "checkLockTypeVisual",
          "event": "designationTypeNotEquals",
          "stateTo": "standby"
        },
        "track": {
          "stateFrom": [
            "lockCue",
            "lockTarget",
            "losLock",
            "visualLock"
          ],
          "event": "targetDetected",
          "stateTo": "track",
          "actions": {
            "setTrackMode": {},
            "setFsmActive": {
              "fsm": "lock",
              "active": false
            }
          }
        },
        "failedLock": {
          "stateFrom": [
            "lockCue",
            "lockTarget",
            "losLock",
            "visualLock"
          ],
          "event": "scanFinished",
          "stateTo": "standby",
          "actions": {
            "setStandbyMode": {},
            "setFsmActive": {
              "fsm": "lock",
              "active": false
            }
          }
        },
        "finishedTrack": {
          "stateFrom": "track",
          "event": "targetsOfInterestCleanup",
          "stateTo": "standby",
          "command": "designateTarget",
          "actions": {
            "setStandbyMode": {},
            "setFsmActive": {
              "fsm": "track",
              "active": false
            }
          }
        }
      }
    },
    "lock": {
      "stateInit": "lock",
      "transitions": {
        "scan": {
          "event": "update",
          "actions": {
            "scan": {}
          }
        },
        "detect": {
          "event": "targetInSight",
          "actions": {
            "detectTarget": {
              "ignoreOwnWeapon": true,
              "rangeMult": 1.0
            }
          }
        },
        "aim": {
          "command": "aim",
          "actions": {
            "setCenterToAim": {}
          }
        }
      }
    },
    "track": {
      "stateInit": "track",
      "transitions": {
        "detect": {
          "event": "update",
          "actions": {
            "detectTarget": {
              "ignoreOwnWeapon": true,
              "rangeMult": 1.0
            }
          }
        },
        "track": {
          "event": "targetDetected",
          "actions": {
            "updateActiveTargetOfInterest": {
              "dirFilter": {
                "rateLim": [
                  0.78,
                  0.78
                ],
                "limTimeOut": 1.0,
                "inertialTimeMax": 1.0
              },
              "distFilter": {
                "accelLim": [
                  150.0,
                  150.0
                ],
                "limTimeOut": 1.0,
                "inertialTimeMax": 1.0
              }
            },
            "setCenterAzimuth": {
              "source": "activeTargetOfInterest"
            },
            "setCenterElevation": {
              "source": "activeTargetOfInterest"
            },
            "setDistGatePos": {
              "source": "activeTargetOfInterest",
              "width": 0.0
            },
            "setAngularSize": {
              "source": "activeTargetOfInterest",
              "value": 1.25
            }
          }
        },
        "extrapolate": {
          "event": "targetNotDetected",
          "actions": {
            "extrapolateTargetsOfInterest": {},
            "clearTargetsOfInterest": {
              "timeOut": 2.0,
              "dirRateLim": 0.78
            },
            "setCenterAzimuth": {
              "source": "activeTargetOfInterest"
            },
            "setCenterElevation": {
              "source": "activeTargetOfInterest"
            },
            "setDistGatePos": {
              "source": "activeTargetOfInterest",
              "width": 0.0
            }
          }
        }
      }
    }
  }
}