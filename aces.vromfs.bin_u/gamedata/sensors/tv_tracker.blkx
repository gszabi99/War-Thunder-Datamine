{
  "type": "radar",
  "canMoveCamera": true,
  "name": "Auto tracker",
  "showOnHud": false,
  "designationSourceType": 0,
  "transivers": {
    "track": {
      "power": 60.0,
      "active": false,
      "band": -1,
      "rcs": 3000.0,
      "range": 10000.0,
      "rangeMax": 15000.0,
      "visibilityType": "optic",
      "antenna": {
        "angleHalfSens": 0.05,
        "sideLobesSensitivity": -120.0
      }
    }
  },
  "scanPatterns": {
    "lock": {
      "type": "cone",
      "period": 0.2,
      "width": 0.01,
      "azimuthLimits": [
        -180.0,
        180.0
      ],
      "elevationLimits": [
        -180.0,
        180.0
      ],
      "yawStabLimit": 180.0,
      "pitchStabLimit": 180.0,
      "rollStabLimit": [
        180.0,
        180.0
      ]
    },
    "losLock": {
      "type": "cone",
      "period": 0.2,
      "width": 0.01,
      "azimuthLimits": [
        -180.0,
        180.0
      ],
      "elevationLimits": [
        -180.0,
        180.0
      ],
      "yawStabLimit": 180.0,
      "pitchStabLimit": 180.0,
      "rollStabLimit": [
        180.0,
        180.0
      ]
    },
    "track": {
      "type": "no",
      "azimuthLimits": [
        -180.0,
        180.0
      ],
      "elevationLimits": [
        -180.0,
        180.0
      ],
      "yawStabLimit": 180.0,
      "pitchStabLimit": 180.0,
      "rollStabLimit": 180.0,
      "pitchStabMandatory": true
    }
  },
  "signals": {
    "track": {
      "anglesFinder": true,
      "rangeFinder": true,
      "groundClutter": false,
      "aircraftAsTarget": true,
      "groundVehiclesAsTarget": true,
      "angularAccuracy": 0.0,
      "distanceAccuracy": 0.0,
      "detectWeapon": false,
      "distance": {
        "presents": true,
        "minValue": 100.0,
        "width": 200.0,
        "maxValue": 15000.0
      }
    }
  },
  "fsms": {
    "main": {
      "stateInit": "init",
      "actionsTemplates": {
        "init": {
          "setEnabled": {
            "value": false
          },
          "setTargetDesignationRange": [
            {},
            {
              "azimuthRange": [
                -180.0,
                180.0
              ],
              "elevationRange": [
                -180.0,
                180.0
              ],
              "distanceRange": [
                0.0,
                15000.0
              ]
            }
          ],
          "setTransiver": {
            "transiver": "track"
          },
          "setSignal": {
            "signal": "track"
          }
        },
        "setStandbyMode": {
          "setModeName": {
            "name": "standby"
          },
          "setEnabled": {
            "value": false
          },
          "setScanPattern": {
            "scanPattern": "lock"
          },
          "setCenterAzimuth": {
            "source": "constant",
            "value": 0.0
          },
          "clearTargetsOfInterest": {},
          "setFsmActive": [
            {
              "fsm": "lock",
              "active": false
            },
            {
              "fsm": "track",
              "active": false
            }
          ]
        },
        "setLockMode": {
          "setModeName": {
            "name": "acquisition"
          },
          "setWeaponDetectionAllowance": {
            "source": "const",
            "value": true
          },
          "setDesignatedTarget": {
            "set": true
          },
          "setEnabled": {
            "value": true
          },
          "setScanPattern": {
            "scanPattern": "lock"
          },
          "resetScanPhase": {},
          "setCenterAzimuth": {
            "source": "targetDesignation"
          },
          "clearTargetsOfInterest": {},
          "setFsmActive": {
            "fsm": "lock",
            "active": true
          },
          "setCenterElevation": {
            "source": "targetDesignation"
          },
          "setDistGatePos": {
            "source": "targetDesignation",
            "width": 50.0
          }
        },
        "setLosVisualPreLockMode": {
          "setEnabled": {
            "value": true
          },
          "setScanPattern": {
            "scanPattern": "losLock"
          },
          "resetScanPhase": {},
          "clearTargetsOfInterest": {},
          "setCenterAzimuth": {
            "source": "targetDesignation"
          },
          "setCenterElevation": {
            "source": "targetDesignation"
          },
          "setFsmActive": {
            "fsm": "lock",
            "active": true
          }
        },
        "setLosLockMode": {
          "setModeName": {
            "name": "acquisition"
          },
          "setEnabled": {
            "value": true
          },
          "setWeaponDetectionAllowance": {
            "source": "targetDesignation"
          },
          "setDesignatedTarget": {
            "set": false
          },
          "setDistGatePos": {
            "source": "constRange",
            "pos": 7500.0,
            "width": 15000.0
          }
        },
        "setVisualLockMode": {
          "setModeName": {
            "name": "acquisition"
          },
          "setEnabled": {
            "value": true
          },
          "setWeaponDetectionAllowance": {
            "source": "const",
            "value": true
          },
          "setDesignatedTarget": {
            "set": true
          },
          "setDistGatePos": {
            "source": "targetDesignation",
            "width": 50.0
          }
        },
        "setTrackMode": {
          "setModeName": {
            "name": "track"
          },
          "setEnabled": {
            "value": true
          },
          "setWeaponDetectionAllowance": {
            "source": "const",
            "value": true
          },
          "setDesignatedTarget": {
            "set": false
          },
          "setScanPattern": {
            "scanPattern": "track"
          },
          "clearTargetsOfInterest": {},
          "addTargetOfInterest": {},
          "setLastTargetOfInterestActive": {},
          "updateActiveTargetOfInterest": {},
          "setCenterAzimuth": {
            "source": "activeTargetOfInterest"
          },
          "setCenterElevation": {
            "source": "activeTargetOfInterest"
          },
          "setDistGatePos": {
            "source": "activeTargetOfInterest",
            "width": 0.0
          },
          "setRelSpeedGatePos": {
            "source": "activeTargetOfInterest",
            "width": 0.0
          },
          "setFsmActive": {
            "fsm": "track",
            "active": true
          }
        }
      },
      "transitions": {
        "init": {
          "stateFrom": "init",
          "event": "init",
          "stateTo": "standby",
          "actions": {
            "init": {},
            "setStandbyMode": {}
          }
        },
        "checkLockType0": {
          "stateFrom": "standby",
          "command": "designateTarget",
          "stateTo": "checkLockType0",
          "actions": {
            "checkDesignationTypeEquals": {
              "value": 0
            }
          }
        },
        "lock": {
          "stateFrom": "checkLockType0",
          "event": "designationTypeEquals",
          "stateTo": "lock",
          "actions": {
            "setLockMode": {}
          }
        },
        "checkLockType1": {
          "stateFrom": "checkLockType0",
          "event": "designationTypeNotEquals",
          "stateTo": "checkLockType1",
          "actions": {
            "checkDesignationTypeEquals": {
              "value": 1
            }
          }
        },
        "losLock": {
          "stateFrom": "checkLockType1",
          "event": "designationTypeEquals",
          "stateTo": "losLock",
          "actions": {
            "setLosVisualPreLockMode": {},
            "setLosLockMode": {}
          }
        },
        "checkLockType2": {
          "stateFrom": "checkLockType1",
          "event": "designationTypeNotEquals",
          "stateTo": "checkLockType2",
          "actions": {
            "checkDesignationTypeEquals": {
              "value": 2
            }
          }
        },
        "visualLock": {
          "stateFrom": "checkLockType2",
          "event": "designationTypeEquals",
          "stateTo": "visualLock",
          "actions": {
            "setLosVisualPreLockMode": {},
            "setVisualLockMode": {}
          }
        },
        "finishLockTypeCheck": {
          "stateFrom": "checkLockType2",
          "event": "designationTypeNotEquals",
          "stateTo": "standby"
        },
        "track": {
          "stateFrom": [
            "lock",
            "losLock",
            "visualLock"
          ],
          "event": "targetDetected",
          "stateTo": "track",
          "actions": {
            "setTrackMode": {},
            "setFsmActive": {
              "fsm": "lock",
              "active": false
            }
          }
        },
        "failedLock": {
          "stateFrom": [
            "lock",
            "losLock",
            "visualLock"
          ],
          "event": "scanFinished",
          "stateTo": "standby",
          "actions": {
            "setStandbyMode": {},
            "setFsmActive": {
              "fsm": "lock",
              "active": false
            }
          }
        },
        "finishedTrack": {
          "stateFrom": "track",
          "event": "targetsOfInterestCleanup",
          "stateTo": "standby",
          "command": "designateTarget",
          "actions": {
            "setStandbyMode": {},
            "setFsmActive": {
              "fsm": "track",
              "active": false
            }
          }
        }
      }
    },
    "lock": {
      "stateInit": "lock",
      "transitions": {
        "scan": {
          "event": "update",
          "actions": {
            "scan": {}
          }
        },
        "detect": {
          "event": "targetInSight",
          "actions": {
            "detectTarget": {
              "ignoreOwnWeapon": true,
              "rangeMult": 1.0
            }
          }
        }
      }
    },
    "track": {
      "stateInit": "track",
      "transitions": {
        "detect": {
          "event": "update",
          "actions": {
            "detectTarget": {
              "ignoreOwnWeapon": true,
              "rangeMult": 1.0
            }
          }
        },
        "track": {
          "event": "targetDetected",
          "actions": {
            "updateActiveTargetOfInterest": {
              "dirFilter": {
                "rateLim": [
                  0.78,
                  0.78
                ],
                "limTimeOut": 1.0,
                "inertialTimeMax": 1.0
              },
              "distFilter": {},
              "dopplerSpeedFilter": {}
            },
            "setCenterAzimuth": {
              "source": "activeTargetOfInterest"
            },
            "setCenterElevation": {
              "source": "activeTargetOfInterest"
            },
            "setDistGatePos": {
              "source": "activeTargetOfInterest",
              "width": 0.0
            },
            "setRelSpeedGatePos": {
              "source": "activeTargetOfInterest",
              "width": 0.0
            }
          }
        },
        "extrapolate": {
          "event": "targetNotDetected",
          "actions": {
            "extrapolateTargetsOfInterest": {},
            "clearTargetsOfInterest": {
              "timeOut": 2.0,
              "dirRateLim": 0.78
            },
            "setCenterAzimuth": {
              "source": "activeTargetOfInterest"
            },
            "setCenterElevation": {
              "source": "activeTargetOfInterest"
            },
            "setDistGatePos": {
              "source": "activeTargetOfInterest",
              "width": 0.0
            },
            "setRelSpeedGatePos": {
              "source": "activeTargetOfInterest",
              "width": 0.0
            }
          }
        }
      }
    }
  }
}