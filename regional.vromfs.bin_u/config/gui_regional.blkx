    "09_07_20_ww_battle_for_the_fulda_gap_popup": {
      "name": "World War: Battle for the Fulda Gap",
      "name_ru": "Мировая война: Битва за Фульдский коридор",
      "name_de": "World War: Schlacht um die Fulda Gap",
      "name_pl": "Wojna Światowa: Bitwa o przesmyk Fulda",
      "name_fr": "Guerre mondiale: La bataille de la Trouée de Fulda",
      "name_cz": "Světová válka: Bitva o mezeru Fulda",
      "name_es": "Guerra Mundial: Batalla por la Brecha de Fulda",
      "name_pt": "Guerra Mundial: Batalha da Passagem de Fulda",
      "name_ko": "세계 대전 시나리오: 풀다 갭 전투",
      "name_zh": "世界大战：富尔达缺口战役",
      "linkText": "More details",
      "linkText_ru": "Подробности",
      "linkText_cs": "Další detaily",
      "linkText_it": "Più dettagli",
      "linkText_pl": "Więcej informacji",
