    "11_07_20_ww_battle_of_the_chinese_farm_popup": {
      "name": "World War: Battle of the “Chinese Farm”",
      "name_ru": "Мировая война: Битва за «Китайскую ферму»",
      "name_de": "World War: Schlacht um die “Chinesische Farm”",
      "name_pl": "Wojna Światowa: Bitwa o “Chińską Farmę”",
      "name_fr": "Guerre mondiale: La bataille de la ferme chinoise",
      "name_cz": "Světová válka: Bitva o “čínskou farmu”",
      "name_es": "Guerra Mundial: Batalla de la “Granja China”",
      "name_pt": "Guerra Mundial: Batalha da Fazenda Chinesa",
      "name_ko": "세계 대전 시나리오: 차이니즈 팜 전투",
      "name_zh": "世界大战：中国农场战役”",
      "linkText": "More details",
      "linkText_ru": "Подробности",
      "linkText_cs": "Další detaily",
      "linkText_it": "Più dettagli",
